            <p:nvPr>
              <p:ph type="body" idx="1"/>
            </p:nvPr>
          </p:nvSpPr>
          <p:spPr/>
          <p:txBody>
            <a:bodyPr/>
            <a:lstStyle/>
            {% for metric in keyMetrics %}
            <a:p>
              <a:pPr>
                <a:buChar char="•"/>
              </a:pPr>
              <a:r>
                <a:rPr lang="en-US" sz="2000" b="1">
                  <a:solidFill>
                    <a:schemeClr val="tx1"/>
                  </a:solidFill>
                </a:rPr>
                <a:t>{{ metric.name }}:</a:t>
              </a:r>
              <a:r>
                <a:rPr lang="en-US" sz="2000">
                  <a:solidFill>
                    <a:schemeClr val="tx1"/>
                  </a:solidFill>
                </a:rPr>
                <a:t> {{ metric.current }}</a:t>
              </a:r>
              {% if metric.target %}
              <a:r>
                <a:rPr lang="en-US" sz="1800">
                  <a:solidFill>
                    {% if metric.current >= metric.target %}
                    <a:srgbClr val="00AA00"/>
                    {% else %}
                    <a:srgbClr val="CC0000"/>
                    {% endif %}
                  </a:solidFill>
                </a:rPr>
                <a:t> (Target: {{ metric.target }})</a:t>
              </a:r>
              {% endif %}
            </a:p>
            {% endfor %}
          </p:txBody>
        </p:sp>
      </p:spTree>
