---
to: {{ dest || 'reports' }}/quarterly-review-{{ year }}-Q{{ quarter }}.pptx
inject: false
skipIf: false
variables:
  - companyName: string
  - quarter: number
  - year: number
  - presenter: string
  - executiveSummary: string
  - financialResults: object
  - keyMetrics: array
  - achievements: array
  - challenges: array
  - marketUpdate: string
  - competitorAnalysis: array
  - nextQuarterGoals: array
  - risks: array
  - opportunities: array
filters:
  - currency
  - percentage
  - number
  - date
  - upper
