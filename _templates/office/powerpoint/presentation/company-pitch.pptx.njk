                </a:rPr>
                <a:t>{{ founderName }}</a:t>
              </a:r>
            </a:p>
            <a:p>
              <a:r>
                <a:rPr lang="en-US" sz="1800">
                  <a:solidFill>
                    <a:schemeClr val="tx1"/>
                  </a:solidFill>
                </a:rPr>
                <a:t>{{ contactInfo.email }}</a:t>
              </a:r>
            </a:p>
            <a:p>
              <a:r>
                <a:rPr lang="en-US" sz="1800">
                  <a:solidFill>
                    <a:schemeClr val="tx1"/>
                  </a:solidFill>
                </a:rPr>
                <a:t>{{ contactInfo.phone }}</a:t>
              </a:r>
            </a:p>
            {% if contactInfo.website %}
