    design:tokens [
        design:tokenName "primary-50" ;
        design:tokenValue "#eff6ff" ;
        design:tokenType "color"
    ], [
        design:tokenName "primary-500" ;
        design:tokenValue "#3b82f6" ;
        design:tokenType "color"
    ], [
        design:tokenName "primary-900" ;
        design:tokenValue "#1e3a8a" ;
        design:tokenType "color"
    ], [
        design:tokenName "gray-50" ;
        design:tokenValue "#f9fafb" ;
        design:tokenType "color"
    ], [
        design:tokenName "gray-500" ;
        design:tokenValue "#6b7280" ;
        design:tokenType "color"
    ], [
        design:tokenName "gray-900" ;
        design:tokenValue "#111827" ;
        design:tokenType "color"
    ] .

design:spacing a design:TokenSet ;
    design:category "Spacing" ;
    design:tokens [
        design:tokenName "xs" ;
        design:tokenValue "0.5rem" ;
        design:tokenType "dimension"
    ], [
        design:tokenName "sm" ;
