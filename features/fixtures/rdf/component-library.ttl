design:typography a design:TokenSet ;
    design:category "Typography" ;
    design:tokens [
        design:tokenName "font-family-sans" ;
        design:tokenValue "system-ui, sans-serif" ;
        design:tokenType "fontFamily"
    ], [
        design:tokenName "font-family-mono" ;
        design:tokenValue "ui-monospace, monospace" ;
        design:tokenType "fontFamily"
    ], [
        design:tokenName "font-size-xs" ;
        design:tokenValue "0.75rem" ;
        design:tokenType "dimension"
    ], [
        design:tokenName "font-size-sm" ;
        design:tokenValue "0.875rem" ;
        design:tokenType "dimension"
    ], [
        design:tokenName "font-size-base" ;
        design:tokenValue "1rem" ;
        design:tokenType "dimension"
    ] .

# Component Categories
ui:formControls a ui:ComponentCategory ;
    dct:title "Form Controls" ;
    dct:description "Interactive form input components" ;
    ui:components ui:Button, ui:Input, ui:Checkbox, ui:Radio, ui:Select .

ui:layout a ui:ComponentCategory ;
    dct:title "Layout" ;
    dct:description "Layout and structure components" ;
    ui:components ui:Container, ui:Grid, ui:Card, ui:Modal .

ui:dataDisplay a ui:ComponentCategory ;
    dct:title "Data Display" ;
    dct:description "Components for displaying data" ;
    ui:components ui:Table, ui:List, ui:Badge, ui:Avatar .

ui:feedback a ui:ComponentCategory ;
    dct:title "Feedback" ;
    dct:description "User feedback and status components" ;
    ui:components ui:Spinner, ui:Alert, ui:Toast, ui:ProgressBar .

