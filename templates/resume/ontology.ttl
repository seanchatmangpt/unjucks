#################################################################
#    Object Properties
#################################################################

# Person-Experience Relationships
resume:hasExperience a owl:ObjectProperty ;
    rdfs:label "has experience"@en ;
    rdfs:comment "Relates a person to their professional experience"@en ;
    rdfs:domain resume:Person ;
    rdfs:range resume:Experience .

resume:workedAt a owl:ObjectProperty ;
    rdfs:label "worked at"@en ;
    rdfs:comment "Relates an experience to the organization where it occurred"@en ;
    rdfs:domain resume:Experience ;
    rdfs:range resume:Organization ;
    rdfs:subPropertyOf resume:associatedWith .

resume:associatedWith a owl:ObjectProperty ;
    rdfs:label "associated with"@en ;
    rdfs:comment "General association between entities"@en .

# Person-Skill Relationships
resume:hasSkill a owl:ObjectProperty ;
    rdfs:label "has skill"@en ;
    rdfs:comment "Relates a person to a skill they possess"@en ;
    rdfs:domain resume:Person ;
    rdfs:range skill:Skill .

resume:skillUsedIn a owl:ObjectProperty ;
    rdfs:label "skill used in"@en ;
    rdfs:comment "Relates a skill to an experience where it was used"@en ;
    rdfs:domain skill:Skill ;
    rdfs:range resume:Experience ;
    owl:inverseOf resume:usedSkill .

resume:usedSkill a owl:ObjectProperty ;
    rdfs:label "used skill"@en ;
    rdfs:comment "Relates an experience to skills that were used"@en ;
    rdfs:domain resume:Experience ;
    rdfs:range skill:Skill ;
    owl:inverseOf resume:skillUsedIn .

# Person-Education Relationships
resume:hasEducation a owl:ObjectProperty ;
    rdfs:label "has education"@en ;
    rdfs:comment "Relates a person to their educational background"@en ;
    rdfs:domain resume:Person ;
    rdfs:range resume:Education .

resume:studiedAt a owl:ObjectProperty ;
    rdfs:label "studied at"@en ;
    rdfs:comment "Relates education to the institution where it was obtained"@en ;
    rdfs:domain resume:Education ;
    rdfs:range resume:EducationalInstitution .

# Job-Requirement Relationships
job:hasRequirement a owl:ObjectProperty ;
    rdfs:label "has requirement"@en ;
    rdfs:comment "Relates a job posting to its requirements"@en ;
    rdfs:domain job:JobPosting ;
    rdfs:range job:JobRequirement .

job:requiresSkill a owl:ObjectProperty ;
    rdfs:label "requires skill"@en ;
    rdfs:comment "Relates a job to a required skill"@en ;
    rdfs:domain job:JobPosting ;
    rdfs:range skill:Skill ;
    rdfs:subPropertyOf job:hasRequirement .

job:requiresExperience a owl:ObjectProperty ;
    rdfs:label "requires experience"@en ;
    rdfs:comment "Relates a job to required experience"@en ;
    rdfs:domain job:JobPosting ;
    rdfs:range resume:Experience ;
    rdfs:subPropertyOf job:hasRequirement .

# Project and Achievement Relationships
resume:hasProject a owl:ObjectProperty ;
    rdfs:label "has project"@en ;
    rdfs:comment "Relates a person to their projects"@en ;
    rdfs:domain resume:Person ;
    rdfs:range resume:Project .

resume:hasAchievement a owl:ObjectProperty ;
    rdfs:label "has achievement"@en ;
    rdfs:comment "Relates a person to their achievements"@en ;
    rdfs:domain resume:Person ;
    rdfs:range resume:Achievement .

resume:projectUsedSkill a owl:ObjectProperty ;
    rdfs:label "project used skill"@en ;
    rdfs:comment "Relates a project to skills used in it"@en ;
    rdfs:domain resume:Project ;
    rdfs:range skill:Skill .

# Skill Relationships
skill:relatedTo a owl:ObjectProperty ;
    rdfs:label "related to"@en ;
    rdfs:comment "Relates skills that are conceptually related"@en ;
    rdfs:domain skill:Skill ;
    rdfs:range skill:Skill ;
