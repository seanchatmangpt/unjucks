resume:Candidate a owl:Class ;
    rdfs:label "Job Candidate"@en ;
    rdfs:comment "A person seeking employment opportunities"@en ;
    rdfs:subClassOf resume:Professional .

# Experience and Career
resume:Experience a owl:Class ;
    rdfs:label "Professional Experience"@en ;
