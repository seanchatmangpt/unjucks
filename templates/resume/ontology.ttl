skill:buildsOn a owl:ObjectProperty ;
    rdfs:label "builds on"@en ;
    rdfs:comment "Indicates a skill builds upon another skill"@en ;
    rdfs:domain skill:Skill ;
    rdfs:range skill:Skill ;
    owl:inverseOf skill:prerequisiteFor .

