---
to: "{{ candidateName | slug }}-{{ industry }}-resume.html"
inject: false
skipIf: false
---
<!DOCTYPE html>
<html lang="en" vocab="http://schema.org/" typeof="Person">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title property="name">{{ candidateName }} - {{ jobTitle }} Resume</title>
    
    <!-- Semantic metadata -->
    <meta property="jobTitle" content="{{ jobTitle }}">
    <meta property="email" content="{{ email }}">
    <meta property="telephone" content="{{ phone }}">
    <meta property="address" content="{{ location }}">
    <meta name="description" content="Professional resume for {{ candidateName }}, {{ jobTitle }} in {{ industry }}">
    <meta name="keywords" content="{{ candidateName }}, {{ jobTitle }}, {{ industry }}, resume, CV">
    
    <!-- Open Graph metadata -->
    <meta property="og:title" content="{{ candidateName }} - {{ jobTitle }}">
    <meta property="og:description" content="Professional resume for {{ candidateName }}">
    <meta property="og:type" content="profile">
    <meta property="og:url" content="{{ portfolioUrl }}">
    
    <!-- JSON-LD structured data -->
    {% if includeSemanticData %}
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Person",
        "name": "{{ candidateName }}",
        "jobTitle": "{{ jobTitle }}",
        "email": "{{ email }}",
        "telephone": "{{ phone }}",
        "address": {
            "@type": "PostalAddress",
            "addressLocality": "{{ location }}"
        },
        "url": "{{ portfolioUrl }}",
        "sameAs": [
            "{{ linkedinUrl }}",
            "{{ githubUrl }}"
        ],
        "worksFor": {
            "@type": "Organization",
            "name": "TechCorp Inc."
        },
        "alumniOf": {
            "@type": "EducationalOrganization",
            "name": "University of Technology"
        },
        "knowsAbout": [
            {% if industry == 'software' %}
            "Software Engineering", "Full Stack Development", "System Design", "Agile Methodologies"
            {% elif industry == 'data-science' %}
            "Data Science", "Machine Learning", "Statistical Analysis", "Data Visualization"
            {% elif industry == 'product' %}
            "Product Management", "Product Strategy", "Roadmap Development", "Cross-functional Leadership"
            {% else %}
            "Professional Development", "Team Collaboration", "Problem Solving"
            {% endif %}
        ]
    }
    </script>
    {% endif %}
    
    <style>
        /* Modern CSS with semantic structure */
        :root {
            --primary-color: #2563eb;
            --secondary-color: #64748b;
            --accent-color: #0ea5e9;
            --text-primary: #1e293b;
            --text-secondary: #475569;
            --background: #ffffff;
            --surface: #f8fafc;
            --border: #e2e8f0;
            --shadow: 0 1px 3px 0 rgb(0 0 0 / 0.1);
        }
        
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        body {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif;
            line-height: 1.6;
            color: var(--text-primary);
            background: var(--background);
            font-size: 16px;
        }
        
        .resume-container {
            max-width: 850px;
            margin: 0 auto;
            padding: 2rem;
            background: var(--background);
            box-shadow: var(--shadow);
            border-radius: 8px;
        }
        
        /* Header with semantic styling */
        .header[typeof="Person"] {
            text-align: center;
            border-bottom: 3px solid var(--primary-color);
            padding-bottom: 2rem;
            margin-bottom: 2rem;
        }
        
        .name[property="name"] {
            font-size: 2.5rem;
            font-weight: 700;
            color: var(--primary-color);
            margin-bottom: 0.5rem;
        }
        
        .job-title[property="jobTitle"] {
            font-size: 1.25rem;
            color: var(--secondary-color);
            margin-bottom: 1rem;
        }
        
        .contact-info {
            display: flex;
            justify-content: center;
            gap: 2rem;
            flex-wrap: wrap;
            font-size: 0.95rem;
        }
        
        .contact-item {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            color: var(--text-secondary);
        }
        
        .contact-item a {
            color: var(--accent-color);
            text-decoration: none;
        }
        
        .contact-item a:hover {
            text-decoration: underline;
        }
        
        /* Section styling with RDF-compatible structure */
        .section {
            margin-bottom: 2rem;
        }
        
        .section-title {
            font-size: 1.4rem;
            font-weight: 600;
            color: var(--primary-color);
            border-bottom: 2px solid var(--border);
            padding-bottom: 0.5rem;
            margin-bottom: 1rem;
        }
        
        .summary[property="description"] {
            font-size: 1.1rem;
            line-height: 1.7;
            color: var(--text-secondary);
            text-align: justify;
        }
        
        /* Experience entries with semantic markup */
        .experience-entry[typeof="WorkExperience"] {
            margin-bottom: 1.5rem;
            padding: 1rem;
            background: var(--surface);
            border-radius: 6px;
            border-left: 4px solid var(--accent-color);
        }
        
        .experience-header {
            display: flex;
            justify-content: space-between;
            align-items: flex-start;
            margin-bottom: 0.5rem;
            flex-wrap: wrap;
            gap: 1rem;
        }
        
        .position[property="jobTitle"] {
            font-size: 1.2rem;
            font-weight: 600;
            color: var(--primary-color);
        }
        
        .company[property="worksFor"] {
            font-weight: 500;
            color: var(--text-primary);
        }
        
        .duration[property="dateCreated"] {
            color: var(--secondary-color);
            font-size: 0.95rem;
        }
        
        .location[property="workLocation"] {
            color: var(--secondary-color);
            font-size: 0.95rem;
        }
        
        .achievements {
            list-style: none;
            margin-top: 0.5rem;
        }
        
        .achievements li {
            position: relative;
            padding-left: 1.5rem;
            margin-bottom: 0.5rem;
            color: var(--text-secondary);
        }
        
        .achievements li::before {
            content: "▶";
            position: absolute;
            left: 0;
            color: var(--accent-color);
            font-size: 0.8rem;
        }
        
        /* Skills grid with semantic categorization */
        .skills-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 1rem;
        }
        
        .skill-category[typeof="SkillSet"] {
            background: var(--surface);
            padding: 1rem;
            border-radius: 6px;
            border: 1px solid var(--border);
        }
        
        .skill-category-title[property="name"] {
            font-weight: 600;
            color: var(--primary-color);
            margin-bottom: 0.5rem;
        }
        
        .skill-list[property="skills"] {
            color: var(--text-secondary);
            font-size: 0.95rem;
        }
        
        /* Education with semantic structure */
        .education-entry[typeof="EducationalOccupationalCredential"] {
            background: var(--surface);
            padding: 1rem;
            border-radius: 6px;
            border-left: 4px solid var(--primary-color);
        }
        
        .degree[property="credentialCategory"] {
            font-weight: 600;
            color: var(--primary-color);
        }
        
        .institution[property="sourceOrganization"] {
            color: var(--text-primary);
            margin: 0.25rem 0;
        }
        
        /* Projects with portfolio links */
        .project-entry[typeof="SoftwareApplication"] {
            background: var(--surface);
            padding: 1rem;
            border-radius: 6px;
            border-left: 4px solid var(--accent-color);
            margin-bottom: 1rem;
        }
        
        .project-title[property="name"] {
            font-weight: 600;
            color: var(--primary-color);
            margin-bottom: 0.5rem;
        }
        
        .project-title a {
            color: inherit;
            text-decoration: none;
        }
        
        .project-title a:hover {
            text-decoration: underline;
        }
        
        .project-description[property="description"] {
            color: var(--text-secondary);
            margin-bottom: 0.5rem;
        }
        
        .tech-stack {
            font-size: 0.9rem;
            color: var(--secondary-color);
            font-style: italic;
        }
        
        /* Responsive design */
        @media (max-width: 768px) {
            .resume-container {
                padding: 1rem;
                margin: 1rem;
            }
            
            .name {
                font-size: 2rem;
            }
            
            .contact-info {
                flex-direction: column;
                gap: 0.5rem;
            }
            
            .experience-header {
                flex-direction: column;
                gap: 0.5rem;
            }
            
            .skills-grid {
                grid-template-columns: 1fr;
            }
        }
        
        @media print {
            .resume-container {
                box-shadow: none;
                margin: 0;
                padding: 1rem;
            }
            
            .contact-item a {
                color: var(--text-primary);
            }
        }
    </style>
</head>

<body>
    <div class="resume-container">
        <!-- Header with semantic markup -->
        <header class="header" typeof="Person">
            <h1 class="name" property="name">{{ candidateName }}</h1>
            <div class="job-title" property="jobTitle">{{ jobTitle }}</div>
            
            <div class="contact-info">
                <div class="contact-item">
                    <span>📧</span>
                    <a href="mailto:{{ email }}" property="email">{{ email }}</a>
                </div>
                <div class="contact-item">
                    <span>📞</span>
                    <span property="telephone">{{ phone }}</span>
                </div>
                <div class="contact-item">
                    <span>📍</span>
                    <span property="address">{{ location }}</span>
                </div>
                <div class="contact-item">
                    <span>💼</span>
                    <a href="{{ linkedinUrl }}" property="sameAs" target="_blank" rel="noopener">LinkedIn</a>
                </div>
                <div class="contact-item">
                    <span>🔗</span>
                    <a href="{{ githubUrl }}" property="sameAs" target="_blank" rel="noopener">GitHub</a>
                </div>
                <div class="contact-item">
                    <span>🌐</span>
                    <a href="{{ portfolioUrl }}" property="url" target="_blank" rel="noopener">Portfolio</a>
                </div>
            </div>
        </header>

        <!-- Professional Summary -->
        <section class="section">
            <h2 class="section-title">Professional Summary</h2>
            <p class="summary" property="description">
                {% if industry == 'software' %}
                Experienced software engineer with expertise in full-stack development, system design, and agile methodologies. 
                Proven track record of delivering scalable solutions and mentoring development teams with a focus on code quality and performance optimization.
                {% elif industry == 'data-science' %}
                Data scientist with strong background in machine learning, statistical analysis, and data visualization. 
                Experienced in transforming complex datasets into actionable business insights using modern data science tools and methodologies.
                {% elif industry == 'product' %}
                Product manager with experience in product strategy, roadmap development, and cross-functional team leadership. 
                Skilled at bridging technical and business requirements to deliver user-centric solutions that drive business growth.
                {% elif industry == 'design' %}
                Creative designer with expertise in user experience, visual design, and design systems. 
                Passionate about creating intuitive and accessible digital experiences that delight users and achieve business objectives.
                {% else %}
                Professional with diverse experience and strong analytical skills. 
                Dedicated to continuous learning and delivering high-quality results in collaborative environments while adapting to evolving industry needs.
                {% endif %}
            </p>
        </section>

        <!-- Professional Experience -->
        <section class="section">
            <h2 class="section-title">Professional Experience</h2>
            
            <div class="experience-entry" typeof="WorkExperience">
                <div class="experience-header">
                    <div>
                        <div class="position" property="jobTitle">Senior {{ jobTitle }}</div>
                        <div class="company" property="worksFor">TechCorp Inc.</div>
                    </div>
                    <div>
                        <div class="duration" property="dateCreated">2023 - Present</div>
                        <div class="location" property="workLocation">{{ location }}</div>
                    </div>
                </div>
                <ul class="achievements">
                    <li>Led development of microservices architecture serving 1M+ users</li>
                    <li>Implemented CI/CD pipelines reducing deployment time by 60%</li>
                    <li>Mentored 5 junior engineers and conducted technical interviews</li>
                    <li>Collaborated with product and design teams on feature development</li>
                </ul>
            </div>
            
            <div class="experience-entry" typeof="WorkExperience">
                <div class="experience-header">
                    <div>
                        <div class="position" property="jobTitle">{{ jobTitle }}</div>
                        <div class="company" property="worksFor">InnovateCo</div>
                    </div>
                    <div>
                        <div class="duration" property="dateCreated">2021 - 2023</div>
                        <div class="location" property="workLocation">Remote</div>
                    </div>
                </div>
                <ul class="achievements">
                    <li>Developed full-stack applications using modern frameworks</li>
                    <li>Optimized database queries improving application performance by 40%</li>
                    <li>Participated in agile development processes and code reviews</li>
                    <li>Contributed to technical documentation and best practices</li>
                </ul>
            </div>
        </section>

        <!-- Education -->
        <section class="section">
            <h2 class="section-title">Education</h2>
            <div class="education-entry" typeof="EducationalOccupationalCredential">
                <div class="degree" property="credentialCategory">Bachelor of Science in Computer Science</div>
                <div class="institution" property="sourceOrganization">University of Technology</div>
                <div class="duration">2017 - 2021 | GPA: 3.8/4.0</div>
                <div class="coursework">Relevant coursework: Data Structures, Algorithms, Software Engineering, Database Systems</div>
            </div>
        </section>

        <!-- Technical Skills -->
        <section class="section">
            <h2 class="section-title">Technical Skills</h2>
            <div class="skills-grid">
                <div class="skill-category" typeof="SkillSet">
                    <div class="skill-category-title" property="name">Programming</div>
                    <div class="skill-list" property="skills">JavaScript, Python, Java, TypeScript, Go</div>
                </div>
                <div class="skill-category" typeof="SkillSet">
                    <div class="skill-category-title" property="name">Frameworks</div>
                    <div class="skill-list" property="skills">React, Node.js, Django, Spring Boot, Express</div>
                </div>
                <div class="skill-category" typeof="SkillSet">
                    <div class="skill-category-title" property="name">Databases</div>
                    <div class="skill-list" property="skills">PostgreSQL, MongoDB, Redis, Elasticsearch</div>
                </div>
                <div class="skill-category" typeof="SkillSet">
                    <div class="skill-category-title" property="name">Cloud/DevOps</div>
                    <div class="skill-list" property="skills">AWS, Docker, Kubernetes, Jenkins, Terraform</div>
                </div>
            </div>
        </section>

        <!-- Notable Projects -->
        <section class="section">
            <h2 class="section-title">Notable Projects</h2>
            
            <div class="project-entry" typeof="SoftwareApplication">
                <h3 class="project-title" property="name">
                    <a href="{{ githubUrl }}/ecommerce" target="_blank" rel="noopener">E-commerce Platform</a>
                </h3>
                <p class="project-description" property="description">
                    Full-stack e-commerce solution with microservices architecture, payment processing, and real-time inventory management.
                </p>
                <div class="tech-stack">Technologies: React, Node.js, PostgreSQL, Docker, AWS</div>
            </div>
            
            <div class="project-entry" typeof="SoftwareApplication">
                <h3 class="project-title" property="name">
                    <a href="{{ portfolioUrl }}/analytics" target="_blank" rel="noopener">Data Analytics Dashboard</a>
                </h3>
                <p class="project-description" property="description">
                    Real-time analytics dashboard processing 100k+ events/hour with interactive visualizations and custom reporting.
                </p>
                <div class="tech-stack">Technologies: Python, Django, D3.js, PostgreSQL, Redis</div>
            </div>
        </section>

        <!-- Certifications -->
        <section class="section">
            <h2 class="section-title">Certifications</h2>
            <ul class="achievements">
                <li>AWS Certified Solutions Architect - Associate (2023)</li>
                <li>Google Cloud Professional Cloud Architect (2022)</li>
                <li>Certified Kubernetes Administrator (CKA) (2021)</li>
            </ul>
        </section>
    </div>

    <!-- Semantic metadata footer -->
    <footer style="text-align: center; padding: 1rem; color: #64748b; font-size: 0.875rem;">
        Generated: {{ new Date().toLocaleDateString() }} | Semantic Resume Format | Industry: {{ industry | title }}
    </footer>
</body>
</html>