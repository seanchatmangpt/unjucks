    rdfs:range xsd:anyURI .

person:hasGitHub a owl:DatatypeProperty ;
    rdfs:label "has GitHub profile" ;
    rdfs:comment "The GitHub profile URL of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:anyURI .

person:hasPortfolio a owl:DatatypeProperty ;
    rdfs:label "has portfolio" ;
    rdfs:comment "The portfolio website URL of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:anyURI .

person:hasSummary a owl:DatatypeProperty ;
    rdfs:label "has summary" ;
    rdfs:comment "A professional summary or objective" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

# ===========================
# SKILL-RELATED PROPERTIES
# ===========================

person:hasSkill a owl:ObjectProperty ;
    rdfs:label "has skill" ;
    rdfs:comment "A skill that a person possesses" ;
    rdfs:domain person:Person ;
    rdfs:range skill:Skill .

person:hasSkillProficiency a owl:ObjectProperty ;
    rdfs:label "has skill proficiency" ;
    rdfs:comment "A skill proficiency that a person has" ;
    rdfs:domain person:Person ;
    rdfs:range person:SkillProficiency .

person:proficiencyLevel a owl:DatatypeProperty ;
    rdfs:label "proficiency level" ;
    rdfs:comment "The proficiency level for a skill" ;
    rdfs:domain person:SkillProficiency ;
    rdfs:range xsd:string .

person:yearsExperience a owl:DatatypeProperty ;
    rdfs:label "years experience" ;
    rdfs:comment "Years of experience with a skill" ;
    rdfs:domain person:SkillProficiency ;
    rdfs:range xsd:decimal .

person:skillFor a owl:ObjectProperty ;
    rdfs:label "skill for" ;
    rdfs:comment "The skill this proficiency is for" ;
    rdfs:domain person:SkillProficiency ;
    rdfs:range skill:Skill .

person:lastUsed a owl:DatatypeProperty ;
    rdfs:label "last used" ;
    rdfs:comment "When a skill was last used" ;
    rdfs:domain person:SkillProficiency ;
    rdfs:range xsd:date .

# ===========================
# EXPERIENCE PROPERTIES
# ===========================

person:hasExperience a owl:ObjectProperty ;
