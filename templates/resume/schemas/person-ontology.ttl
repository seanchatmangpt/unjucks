@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix person: <http://ontology.resume/person#> .
@prefix skill: <http://ontology.resume/skill#> .
@prefix job: <http://ontology.resume/job#> .
@prefix company: <http://ontology.resume/company#> .
@prefix education: <http://ontology.resume/education#> .

# Person and Resume Ontology for Resume Matching
# Production-ready schema for personal profiles, experience, and education

# ===========================
# ONTOLOGY METADATA
# ===========================

<http://ontology.resume/person> a owl:Ontology ;
    dcterms:title "Person and Resume Ontology" ;
    dcterms:description "Comprehensive ontology for personal profiles, work experience, education, and skills" ;
    dcterms:creator "Resume Matching System" ;
    dcterms:created "2025-09-10"^^xsd:date ;
    dcterms:version "1.0" ;
    owl:versionInfo "Production release for resume matching" ;
    owl:imports <http://ontology.resume/job> .

# ===========================
# CORE PERSON CLASSES
# ===========================

person:Person a owl:Class ;
    rdfs:label "Person" ;
    rdfs:comment "An individual person with professional profile" ;
    rdfs:subClassOf foaf:Person .

person:JobSeeker a owl:Class ;
    rdfs:label "Job Seeker" ;
    rdfs:comment "A person actively seeking employment" ;
    rdfs:subClassOf person:Person .

person:Employee a owl:Class ;
    rdfs:label "Employee" ;
    rdfs:comment "A person currently employed" ;
    rdfs:subClassOf person:Person .

person:Freelancer a owl:Class ;
    rdfs:label "Freelancer" ;
    rdfs:comment "A person working as a freelancer" ;
    rdfs:subClassOf person:Person .

person:Entrepreneur a owl:Class ;
    rdfs:label "Entrepreneur" ;
    rdfs:comment "A person who starts and runs businesses" ;
    rdfs:subClassOf person:Person .

# ===========================
# EXPERIENCE CLASSES
# ===========================

person:Experience a owl:Class ;
    rdfs:label "Work Experience" ;
    rdfs:comment "Professional work experience" ;
    rdfs:subClassOf owl:Thing .

person:JobExperience a owl:Class ;
    rdfs:label "Job Experience" ;
    rdfs:comment "Experience from employment at a company" ;
    rdfs:subClassOf person:Experience .

person:ProjectExperience a owl:Class ;
    rdfs:label "Project Experience" ;
    rdfs:comment "Experience from working on specific projects" ;
    rdfs:subClassOf person:Experience .

person:VolunteerExperience a owl:Class ;
    rdfs:label "Volunteer Experience" ;
    rdfs:comment "Experience from volunteer work" ;
    rdfs:subClassOf person:Experience .

person:FreelanceExperience a owl:Class ;
    rdfs:label "Freelance Experience" ;
    rdfs:comment "Experience from freelance work" ;
    rdfs:subClassOf person:Experience .

# ===========================
# PROJECT CLASSES
# ===========================

person:Project a owl:Class ;
    rdfs:label "Project" ;
    rdfs:comment "A specific project worked on" ;
    rdfs:subClassOf owl:Thing .

person:SoftwareProject a owl:Class ;
    rdfs:label "Software Project" ;
    rdfs:comment "A software development project" ;
    rdfs:subClassOf person:Project .

person:ResearchProject a owl:Class ;
    rdfs:label "Research Project" ;
    rdfs:comment "A research or academic project" ;
    rdfs:subClassOf person:Project .

person:OpenSourceProject a owl:Class ;
    rdfs:label "Open Source Project" ;
    rdfs:comment "An open source software project" ;
    rdfs:subClassOf person:SoftwareProject .

# ===========================
# EDUCATION CLASSES
# ===========================

education:Education a owl:Class ;
    rdfs:label "Education" ;
    rdfs:comment "Educational background and qualifications" ;
    rdfs:subClassOf owl:Thing .

education:Degree a owl:Class ;
    rdfs:label "Degree" ;
    rdfs:comment "An academic degree" ;
    rdfs:subClassOf education:Education .

education:Certification a owl:Class ;
    rdfs:label "Certification" ;
    rdfs:comment "A professional certification" ;
    rdfs:subClassOf education:Education .

education:Course a owl:Class ;
    rdfs:label "Course" ;
    rdfs:comment "A completed course or training" ;
    rdfs:subClassOf education:Education .

education:Bootcamp a owl:Class ;
    rdfs:label "Bootcamp" ;
    rdfs:comment "An intensive training program" ;
    rdfs:subClassOf education:Education .

# ===========================
# DEGREE LEVEL CLASSES
# ===========================

education:AssociateDegree a owl:Class ;
    rdfs:label "Associate Degree" ;
    rdfs:comment "A two-year undergraduate degree" ;
    rdfs:subClassOf education:Degree .

education:BachelorDegree a owl:Class ;
    rdfs:label "Bachelor Degree" ;
    rdfs:comment "A four-year undergraduate degree" ;
    rdfs:subClassOf education:Degree .

education:MasterDegree a owl:Class ;
    rdfs:label "Master Degree" ;
    rdfs:comment "A graduate degree" ;
    rdfs:subClassOf education:Degree .

education:DoctorateDegree a owl:Class ;
    rdfs:label "Doctorate Degree" ;
    rdfs:comment "A doctoral degree" ;
    rdfs:subClassOf education:Degree .

# ===========================
# INSTITUTION CLASSES
# ===========================

education:Institution a owl:Class ;
    rdfs:label "Educational Institution" ;
    rdfs:comment "An educational institution" ;
    rdfs:subClassOf foaf:Organization .

education:University a owl:Class ;
    rdfs:label "University" ;
    rdfs:comment "A university" ;
    rdfs:subClassOf education:Institution .

education:College a owl:Class ;
    rdfs:label "College" ;
    rdfs:comment "A college" ;
    rdfs:subClassOf education:Institution .

education:TechnicalSchool a owl:Class ;
    rdfs:label "Technical School" ;
    rdfs:comment "A technical or vocational school" ;
    rdfs:subClassOf education:Institution .

education:OnlineProvider a owl:Class ;
    rdfs:label "Online Education Provider" ;
    rdfs:comment "An online education platform" ;
    rdfs:subClassOf education:Institution .

# ===========================
# SKILL PROFICIENCY CLASSES
# ===========================

person:SkillProficiency a owl:Class ;
    rdfs:label "Skill Proficiency" ;
    rdfs:comment "A person's proficiency in a specific skill" ;
    rdfs:subClassOf owl:Thing .

person:BeginnerLevel a owl:Class ;
    rdfs:label "Beginner Level" ;
    rdfs:comment "Beginner proficiency level" ;
    rdfs:subClassOf person:SkillProficiency .

person:IntermediateLevel a owl:Class ;
    rdfs:label "Intermediate Level" ;
    rdfs:comment "Intermediate proficiency level" ;
    rdfs:subClassOf person:SkillProficiency .

person:AdvancedLevel a owl:Class ;
    rdfs:label "Advanced Level" ;
    rdfs:comment "Advanced proficiency level" ;
    rdfs:subClassOf person:SkillProficiency .

person:ExpertLevel a owl:Class ;
    rdfs:label "Expert Level" ;
    rdfs:comment "Expert proficiency level" ;
    rdfs:subClassOf person:SkillProficiency .

# ===========================
# BASIC PERSON PROPERTIES
# ===========================

person:hasFirstName a owl:DatatypeProperty ;
    rdfs:label "has first name" ;
    rdfs:comment "The first name of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

person:hasLastName a owl:DatatypeProperty ;
    rdfs:label "has last name" ;
    rdfs:comment "The last name of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

person:hasFullName a owl:DatatypeProperty ;
    rdfs:label "has full name" ;
    rdfs:comment "The full name of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

person:hasEmail a owl:DatatypeProperty ;
    rdfs:label "has email" ;
    rdfs:comment "The email address of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

person:hasPhone a owl:DatatypeProperty ;
    rdfs:label "has phone" ;
    rdfs:comment "The phone number of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

person:hasAddress a owl:DatatypeProperty ;
    rdfs:label "has address" ;
    rdfs:comment "The address of a person" ;
    rdfs:domain person:Person ;
    rdfs:range xsd:string .

person:hasLinkedIn a owl:DatatypeProperty ;
    rdfs:label "has LinkedIn profile" ;
    rdfs:comment "The LinkedIn profile URL of a person" ;
