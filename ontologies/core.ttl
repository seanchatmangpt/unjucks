@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix kgen: <http://kgen.ai/ontology/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

# ==============================================================================
# KGEN Core Ontology - Knowledge Graph Engine Foundation
# ==============================================================================

<http://kgen.ai/ontology/> a owl:Ontology ;
    dcterms:title "KGEN Core Ontology"@en ;
    dcterms:description "Core ontology defining knowledge graph engine concepts for deterministic artifact generation"@en ;
    dcterms:creator "KGEN System"@en ;
    dcterms:created "2025-09-12"^^xsd:date ;
    dcterms:modified "2025-09-12"^^xsd:date ;
    owl:versionInfo "1.0.0"@en ;
    owl:imports <http://www.w3.org/ns/prov#> ;
    rdfs:seeAlso <https://kgen.ai/> ;
    dcterms:license <https://opensource.org/licenses/MIT> .

# ==============================================================================
# Core Classes - Knowledge Graphs and Data
# ==============================================================================

kgen:KnowledgeGraph a owl:Class ;
    rdfs:label "Knowledge Graph"@en ;
    rdfs:comment "An RDF knowledge graph containing structured data"@en ;
    rdfs:subClassOf prov:Collection ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Knowledge_graph> .

kgen:Dataset a owl:Class ;
    rdfs:label "Dataset"@en ;
    rdfs:comment "A collection of related data"@en ;
    rdfs:subClassOf prov:Collection .

kgen:Ontology a owl:Class ;
    rdfs:label "KGEN Ontology"@en ;
    rdfs:comment "An ontology used within the KGEN system"@en ;
    rdfs:subClassOf owl:Ontology .

kgen:Schema a owl:Class ;
    rdfs:label "Data Schema"@en ;
    rdfs:comment "A schema defining data structure"@en ;
    rdfs:subClassOf owl:Thing .

# ==============================================================================
# Artifact Classes
# ==============================================================================

kgen:Artifact a owl:Class ;
    rdfs:label "Generated Artifact"@en ;
    rdfs:comment "An artifact generated by the KGEN system"@en ;
    rdfs:subClassOf prov:Entity ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Software_artifact> .

kgen:CodeArtifact a owl:Class ;
    rdfs:label "Code Artifact"@en ;
    rdfs:comment "A generated code artifact (source file)"@en ;
    rdfs:subClassOf kgen:Artifact .

kgen:ConfigurationArtifact a owl:Class ;
    rdfs:label "Configuration Artifact"@en ;
    rdfs:comment "A generated configuration file"@en ;
    rdfs:subClassOf kgen:Artifact .

kgen:DocumentationArtifact a owl:Class ;
    rdfs:label "Documentation Artifact"@en ;
    rdfs:comment "Generated documentation"@en ;
    rdfs:subClassOf kgen:Artifact .

kgen:TestArtifact a owl:Class ;
    rdfs:label "Test Artifact"@en ;
    rdfs:comment "Generated test code or test data"@en ;
    rdfs:subClassOf kgen:Artifact .

# ==============================================================================
# Template Classes
# ==============================================================================

kgen:Template a owl:Class ;
    rdfs:label "Generation Template"@en ;
    rdfs:comment "A template used for generating artifacts"@en ;
    rdfs:subClassOf prov:Plan .

kgen:TemplateEngine a owl:Class ;
    rdfs:label "Template Engine"@en ;
    rdfs:comment "A template processing engine"@en ;
    rdfs:subClassOf prov:SoftwareAgent .

kgen:TemplateVariable a owl:Class ;
    rdfs:label "Template Variable"@en ;
    rdfs:comment "A variable used in template rendering"@en ;
    rdfs:subClassOf owl:Thing .

kgen:TemplateFilter a owl:Class ;
    rdfs:label "Template Filter"@en ;
    rdfs:comment "A filter function for template rendering"@en ;
    rdfs:subClassOf owl:Thing .

# ==============================================================================
# Generation Process Classes
# ==============================================================================

kgen:GenerationActivity a owl:Class ;
    rdfs:label "Generation Activity"@en ;
    rdfs:comment "An activity that generates artifacts from knowledge graphs"@en ;
    rdfs:subClassOf prov:Activity .

kgen:RenderingActivity a owl:Class ;
    rdfs:label "Template Rendering Activity"@en ;
    rdfs:comment "Activity of rendering templates to generate artifacts"@en ;
    rdfs:subClassOf kgen:GenerationActivity .

kgen:ValidationActivity a owl:Class ;
    rdfs:label "Validation Activity"@en ;
    rdfs:comment "Activity of validating artifacts or knowledge graphs"@en ;
    rdfs:subClassOf prov:Activity .

kgen:TransformationActivity a owl:Class ;
    rdfs:label "Data Transformation Activity"@en ;
    rdfs:comment "Activity of transforming data from one format to another"@en ;
    rdfs:subClassOf prov:Activity .

# ==============================================================================
# Configuration Classes
# ==============================================================================

kgen:Configuration a owl:Class ;
    rdfs:label "KGEN Configuration"@en ;
    rdfs:comment "Configuration settings for KGEN operations"@en ;
    rdfs:subClassOf prov:Plan .

kgen:GenerationConfig a owl:Class ;
    rdfs:label "Generation Configuration"@en ;
    rdfs:comment "Configuration for artifact generation"@en ;
    rdfs:subClassOf kgen:Configuration .

kgen:ValidationConfig a owl:Class ;
    rdfs:label "Validation Configuration"@en ;
    rdfs:comment "Configuration for validation operations"@en ;
    rdfs:subClassOf kgen:Configuration .

kgen:OutputConfig a owl:Class ;
    rdfs:label "Output Configuration"@en ;
    rdfs:comment "Configuration for output formatting and structure"@en ;
    rdfs:subClassOf kgen:Configuration .

# ==============================================================================
# Quality and Validation Classes
# ==============================================================================

kgen:SHACLShape a owl:Class ;
    rdfs:label "SHACL Validation Shape"@en ;
    rdfs:comment "A SHACL shape for validating RDF data"@en ;
    rdfs:subClassOf sh:Shape .

kgen:ValidationReport a owl:Class ;
    rdfs:label "Validation Report"@en ;
    rdfs:comment "Report of validation results"@en ;
    rdfs:subClassOf prov:Entity .

kgen:QualityMetric a owl:Class ;
    rdfs:label "Quality Metric"@en ;
    rdfs:comment "A metric for measuring quality"@en ;
    rdfs:subClassOf prov:Entity .

# ==============================================================================
# Error and Drift Classes
# ==============================================================================

kgen:Error a owl:Class ;
    rdfs:label "KGEN Error"@en ;
    rdfs:comment "An error that occurred during KGEN operations"@en ;
    rdfs:subClassOf owl:Thing .

kgen:ValidationError a owl:Class ;
    rdfs:label "Validation Error"@en ;
    rdfs:comment "An error found during validation"@en ;
    rdfs:subClassOf kgen:Error .

kgen:GenerationError a owl:Class ;
    rdfs:label "Generation Error"@en ;
    rdfs:comment "An error that occurred during generation"@en ;
    rdfs:subClassOf kgen:Error .

kgen:ConfigurationDrift a owl:Class ;
    rdfs:label "Configuration Drift"@en ;
    rdfs:comment "Detected drift in configuration"@en ;
    rdfs:subClassOf owl:Thing .

kgen:SchemaDrift a owl:Class ;
    rdfs:label "Schema Drift"@en ;
    rdfs:comment "Detected drift in data schema"@en ;
    rdfs:subClassOf kgen:ConfigurationDrift .

# ==============================================================================
# Core Properties - Basic Information
# ==============================================================================

# Naming and identification
kgen:hasName a owl:DatatypeProperty ;
    rdfs:label "has name"@en ;
    rdfs:comment "The name of an entity"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

kgen:hasURI a owl:DatatypeProperty ;
    rdfs:label "has URI"@en ;
    rdfs:comment "The URI identifier of an entity"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:anyURI .

kgen:hasVersion a owl:DatatypeProperty ;
    rdfs:label "has version"@en ;
    rdfs:comment "Version information"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

kgen:hasDescription a owl:DatatypeProperty ;
    rdfs:label "has description"@en ;
    rdfs:comment "Human-readable description"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

# ==============================================================================
# Artifact Properties
# ==============================================================================

kgen:hasContent a owl:DatatypeProperty ;
    rdfs:label "has content"@en ;
    rdfs:comment "The textual content of an artifact"@en ;
    rdfs:domain kgen:Artifact ;
    rdfs:range xsd:string .

kgen:hasSize a owl:DatatypeProperty ;
    rdfs:label "has size"@en ;
    rdfs:comment "Size of the artifact in bytes"@en ;
    rdfs:domain kgen:Artifact ;
    rdfs:range xsd:integer .

kgen:hasMimeType a owl:DatatypeProperty ;
    rdfs:label "has MIME type"@en ;
    rdfs:comment "MIME type of the artifact"@en ;
    rdfs:domain kgen:Artifact ;
    rdfs:range xsd:string .

kgen:hasEncoding a owl:DatatypeProperty ;
    rdfs:label "has encoding"@en ;
    rdfs:comment "Character encoding of the artifact"@en ;
    rdfs:domain kgen:Artifact ;
    rdfs:range xsd:string .

kgen:hasLanguage a owl:DatatypeProperty ;
    rdfs:label "has programming language"@en ;
    rdfs:comment "Programming language of code artifacts"@en ;
    rdfs:domain kgen:CodeArtifact ;
    rdfs:range xsd:string .

# ==============================================================================
# Generation Properties
# ==============================================================================

kgen:generatedAt a owl:DatatypeProperty ;
    rdfs:label "generated at"@en ;
    rdfs:comment "Timestamp when artifact was generated"@en ;
    rdfs:domain kgen:Artifact ;
    rdfs:range xsd:dateTime .

kgen:generatedBy a owl:ObjectProperty ;
    rdfs:label "generated by"@en ;
    rdfs:comment "Agent or activity that generated the artifact"@en ;
    rdfs:domain kgen:Artifact ;
    rdfs:range [ owl:unionOf ( prov:Agent prov:Activity ) ] .

kgen:usesTemplate a owl:ObjectProperty ;
    rdfs:label "uses template"@en ;
    rdfs:comment "Template used for generation"@en ;
    rdfs:domain kgen:GenerationActivity ;
    rdfs:range kgen:Template .

kgen:hasInputData a owl:ObjectProperty ;
    rdfs:label "has input data"@en ;
    rdfs:comment "Input data for generation"@en ;
    rdfs:domain kgen:GenerationActivity ;
    rdfs:range kgen:KnowledgeGraph .

kgen:produces a owl:ObjectProperty ;
    rdfs:label "produces"@en ;
    rdfs:comment "Artifact produced by the activity"@en ;
    rdfs:domain kgen:GenerationActivity ;
    rdfs:range kgen:Artifact .

# ==============================================================================
# Template Properties
# ==============================================================================

kgen:templateName a owl:DatatypeProperty ;
    rdfs:label "template name"@en ;
    rdfs:comment "Name of the template"@en ;
    rdfs:domain kgen:Template ;
    rdfs:range xsd:string .

kgen:templateEngine a owl:DatatypeProperty ;
    rdfs:label "template engine"@en ;
    rdfs:comment "Template engine used to process this template"@en ;
    rdfs:domain kgen:Template ;
    rdfs:range xsd:string .

kgen:templateContent a owl:DatatypeProperty ;
    rdfs:label "template content"@en ;
    rdfs:comment "The template source code"@en ;
    rdfs:domain kgen:Template ;
    rdfs:range xsd:string .

kgen:hasVariable a owl:ObjectProperty ;
    rdfs:label "has variable"@en ;
    rdfs:comment "Variables used in the template"@en ;
    rdfs:domain kgen:Template ;
    rdfs:range kgen:TemplateVariable .

kgen:generates a owl:DatatypeProperty ;
    rdfs:label "generates"@en ;
    rdfs:comment "Type of content this template generates"@en ;
    rdfs:domain kgen:Template ;
    rdfs:range xsd:string .

# ==============================================================================
# Knowledge Graph Properties
# ==============================================================================

kgen:tripleCount a owl:DatatypeProperty ;
    rdfs:label "triple count"@en ;
    rdfs:comment "Number of RDF triples in the knowledge graph"@en ;
    rdfs:domain kgen:KnowledgeGraph ;
    rdfs:range xsd:integer .

kgen:hasFormat a owl:DatatypeProperty ;
    rdfs:label "has format"@en ;
    rdfs:comment "Serialization format of the knowledge graph"@en ;
    rdfs:domain kgen:KnowledgeGraph ;
    rdfs:range xsd:string .

kgen:usesOntology a owl:ObjectProperty ;
    rdfs:label "uses ontology"@en ;
    rdfs:comment "Ontologies used in the knowledge graph"@en ;
    rdfs:domain kgen:KnowledgeGraph ;
    rdfs:range owl:Ontology .

kgen:hasNamespace a owl:DatatypeProperty ;
    rdfs:label "has namespace"@en ;
    rdfs:comment "RDF namespace used in the graph"@en ;
    rdfs:domain kgen:KnowledgeGraph ;
    rdfs:range xsd:anyURI .

# ==============================================================================
# Configuration Properties
# ==============================================================================

kgen:configName a owl:DatatypeProperty ;
    rdfs:label "configuration name"@en ;
    rdfs:comment "Name of the configuration"@en ;
    rdfs:domain kgen:Configuration ;
    rdfs:range xsd:string .

kgen:configVersion a owl:DatatypeProperty ;
    rdfs:label "configuration version"@en ;
    rdfs:comment "Version of the configuration"@en ;
    rdfs:domain kgen:Configuration ;
    rdfs:range xsd:string .

kgen:outputDirectory a owl:DatatypeProperty ;
    rdfs:label "output directory"@en ;
    rdfs:comment "Directory where artifacts are generated"@en ;
    rdfs:domain kgen:Configuration ;
    rdfs:range xsd:string .

kgen:templateDirectory a owl:DatatypeProperty ;
    rdfs:label "template directory"@en ;
    rdfs:comment "Directory containing templates"@en ;
    rdfs:domain kgen:Configuration ;
    rdfs:range xsd:string .

kgen:hasSettings a owl:DatatypeProperty ;
    rdfs:label "has settings"@en ;
    rdfs:comment "Configuration settings as JSON"@en ;
    rdfs:domain kgen:Configuration ;
    rdfs:range xsd:string .

# ==============================================================================
# Validation Properties
# ==============================================================================

kgen:validatedAt a owl:DatatypeProperty ;
    rdfs:label "validated at"@en ;
    rdfs:comment "Timestamp when validation was performed"@en ;
    rdfs:domain kgen:ValidationReport ;
    rdfs:range xsd:dateTime .

kgen:isValid a owl:DatatypeProperty ;
    rdfs:label "is valid"@en ;
    rdfs:comment "Whether the validation passed"@en ;
    rdfs:domain kgen:ValidationReport ;
    rdfs:range xsd:boolean .

kgen:hasError a owl:ObjectProperty ;
    rdfs:label "has error"@en ;
    rdfs:comment "Validation errors found"@en ;
    rdfs:domain kgen:ValidationReport ;
    rdfs:range kgen:ValidationError .

kgen:errorMessage a owl:DatatypeProperty ;
    rdfs:label "error message"@en ;
    rdfs:comment "Human-readable error message"@en ;
    rdfs:domain kgen:Error ;
    rdfs:range xsd:string .

kgen:errorCode a owl:DatatypeProperty ;
    rdfs:label "error code"@en ;
    rdfs:comment "Machine-readable error code"@en ;
    rdfs:domain kgen:Error ;
    rdfs:range xsd:string .

# ==============================================================================
# Quality Properties
# ==============================================================================

kgen:hasQualityScore a owl:DatatypeProperty ;
    rdfs:label "has quality score"@en ;
    rdfs:comment "Quality score (0.0 to 1.0)"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:decimal .

kgen:measuredAt a owl:DatatypeProperty ;
    rdfs:label "measured at"@en ;
    rdfs:comment "When the metric was measured"@en ;
    rdfs:domain kgen:QualityMetric ;
    rdfs:range xsd:dateTime .

kgen:metricName a owl:DatatypeProperty ;
    rdfs:label "metric name"@en ;
    rdfs:comment "Name of the quality metric"@en ;
    rdfs:domain kgen:QualityMetric ;
    rdfs:range xsd:string .

kgen:metricValue a owl:DatatypeProperty ;
    rdfs:label "metric value"@en ;
    rdfs:comment "Value of the quality metric"@en ;
    rdfs:domain kgen:QualityMetric ;
    rdfs:range xsd:decimal .

# ==============================================================================
# Cryptographic Properties (for cross-ontology compatibility)
# ==============================================================================

kgen:hasHash a owl:ObjectProperty ;
    rdfs:label "has cryptographic hash"@en ;
    rdfs:comment "Cryptographic hash of the entity"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing .  # Will be refined in crypto ontology

kgen:hashValue a owl:DatatypeProperty ;
    rdfs:label "hash value"@en ;
    rdfs:comment "The hash value as hexadecimal string"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

# ==============================================================================
# Provenance Extensions
# ==============================================================================

kgen:deterministicGeneration a owl:DatatypeProperty ;
    rdfs:label "deterministic generation"@en ;
    rdfs:comment "Whether the generation process is deterministic"@en ;
    rdfs:domain kgen:GenerationActivity ;
    rdfs:range xsd:boolean .

kgen:reproducibilitySeed a owl:DatatypeProperty ;
    rdfs:label "reproducibility seed"@en ;
    rdfs:comment "Seed for ensuring reproducible generation"@en ;
    rdfs:domain kgen:GenerationActivity ;
    rdfs:range xsd:string .

kgen:environmentHash a owl:DatatypeProperty ;
    rdfs:label "environment hash"@en ;
    rdfs:comment "Hash of the generation environment"@en ;
    rdfs:domain kgen:GenerationActivity ;
    rdfs:range xsd:string .

# ==============================================================================
# Drift Detection Properties
# ==============================================================================

kgen:detectedAt a owl:DatatypeProperty ;
    rdfs:label "detected at"@en ;
    rdfs:comment "When drift was detected"@en ;
    rdfs:domain kgen:ConfigurationDrift ;
    rdfs:range xsd:dateTime .

kgen:driftType a owl:DatatypeProperty ;
    rdfs:label "drift type"@en ;
    rdfs:comment "Type of drift detected"@en ;
    rdfs:domain kgen:ConfigurationDrift ;
    rdfs:range xsd:string .

kgen:driftSeverity a owl:DatatypeProperty ;
    rdfs:label "drift severity"@en ;
    rdfs:comment "Severity of the drift (low/medium/high)"@en ;
    rdfs:domain kgen:ConfigurationDrift ;
    rdfs:range xsd:string .

kgen:baselineValue a owl:DatatypeProperty ;
    rdfs:label "baseline value"@en ;
    rdfs:comment "Original baseline value"@en ;
    rdfs:domain kgen:ConfigurationDrift ;
    rdfs:range xsd:string .

kgen:currentValue a owl:DatatypeProperty ;
    rdfs:label "current value"@en ;
    rdfs:comment "Current drifted value"@en ;
    rdfs:domain kgen:ConfigurationDrift ;
    rdfs:range xsd:string .

# ==============================================================================
# Named Individuals - Standard Values
# ==============================================================================

# Template Engines
kgen:NunjucksEngine a kgen:TemplateEngine ;
    rdfs:label "Nunjucks Template Engine"@en ;
    kgen:hasName "nunjucks"@en ;
    kgen:hasVersion "3.2.4"@en .

kgen:HandlebarsEngine a kgen:TemplateEngine ;
    rdfs:label "Handlebars Template Engine"@en ;
    kgen:hasName "handlebars"@en .

kgen:MustacheEngine a kgen:TemplateEngine ;
    rdfs:label "Mustache Template Engine"@en ;
    kgen:hasName "mustache"@en .

# Standard MIME Types
kgen:JavaScriptMimeType a owl:NamedIndividual ;
    rdfs:label "JavaScript MIME Type"@en ;
    kgen:hasMimeType "application/javascript"@en .

kgen:TypeScriptMimeType a owl:NamedIndividual ;
    rdfs:label "TypeScript MIME Type"@en ;
    kgen:hasMimeType "application/typescript"@en .

kgen:JSONMimeType a owl:NamedIndividual ;
    rdfs:label "JSON MIME Type"@en ;
    kgen:hasMimeType "application/json"@en .

kgen:YAMLMimeType a owl:NamedIndividual ;
    rdfs:label "YAML MIME Type"@en ;
    kgen:hasMimeType "application/x-yaml"@en .

kgen:MarkdownMimeType a owl:NamedIndividual ;
    rdfs:label "Markdown MIME Type"@en ;
    kgen:hasMimeType "text/markdown"@en .

# Standard Output Formats
kgen:TurtleFormat a owl:NamedIndividual ;
    rdfs:label "Turtle RDF Format"@en ;
    kgen:hasFormat "turtle"@en .

kgen:JSONLD a owl:NamedIndividual ;
    rdfs:label "JSON-LD Format"@en ;
    kgen:hasFormat "json-ld"@en .

kgen:RDFXMLFormat a owl:NamedIndividual ;
    rdfs:label "RDF/XML Format"@en ;
    kgen:hasFormat "rdf-xml"@en .