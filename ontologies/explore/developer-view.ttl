@prefix kmkt: <https://kgen.io/marketplace#> .
@prefix kgen: <https://kgen.io/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# Developer View Schema for implementation details

kmkt:DeveloperView a owl:Class ;
    rdfs:label "Developer View"@en ;
    rdfs:comment "Software developer perspective with APIs, examples, and guides"@en .

kmkt:hasAPIDocumentation a owl:ObjectProperty ;
    rdfs:label "has API documentation"@en ;
    rdfs:domain kmkt:DeveloperView ;
    rdfs:range kmkt:APIDocumentation .

kmkt:hasCodeExample a owl:ObjectProperty ;
    rdfs:label "has code example"@en ;
    rdfs:domain kmkt:DeveloperView ;
    rdfs:range kmkt:CodeExample .

kmkt:hasQuickstartGuide a owl:ObjectProperty ;
    rdfs:label "has quickstart guide"@en ;
    rdfs:domain kmkt:DeveloperView ;
    rdfs:range kmkt:QuickstartGuide .

kmkt:hasImplementationPattern a owl:ObjectProperty ;
    rdfs:label "has implementation pattern"@en ;
    rdfs:domain kmkt:DeveloperView ;
    rdfs:range kmkt:ImplementationPattern .

# API Documentation
kmkt:APIDocumentation a owl:Class ;
    rdfs:label "API Documentation"@en ;
    rdfs:comment "Complete API reference documentation"@en .

kmkt:totalAPIs a owl:DatatypeProperty ;
    rdfs:label "total APIs"@en ;
    rdfs:domain kmkt:APIDocumentation ;
    rdfs:range xsd:integer .

kmkt:hasAuthentication a owl:ObjectProperty ;
    rdfs:label "has authentication"@en ;
