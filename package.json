{
  "name": "@seanchatmangpt/unjucks",
  "version": "2.0.8",
  "type": "module",
  "description": "Nunjucks + Hygen style scaffolding with RDF/Turtle support",
  "main": "./src/cli/index.js",
  "bin": {
    "unjucks": "./bin/unjucks-standalone.cjs"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/cli/index.js",
      "require": "./bin/unjucks.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "bin/",
    "src/",
    "_templates/",
    "types/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "chmod +x bin/unjucks.cjs && chmod +x src/cli/index.js && node scripts/build-system.js",
    "build:enhanced": "node scripts/enhanced-build-system.js",
    "build:watch": "node scripts/enhanced-build-system.js --watch",
    "build:validate": "node scripts/build-system.js",
    "build:prepare": "chmod +x bin/unjucks.cjs && chmod +x src/cli/index.js",
    "dev": "node --watch src/cli/index.js",
    "dev:server": "node -e \"import('./src/lib/build/dev-server.js').then(m => new m.DevelopmentServer().start())\"",
    "test": "node tests/native-test-runner.js",
    "test:vitest": "vitest run --no-coverage",
    "test:watch": "vitest --no-coverage",
    "test:smoke": "node tests/smoke/fast-runner.js",
    "test:smoke:fast": "node tests/smoke/fast-runner.js",
    "test:smoke:critical": "node tests/smoke/critical-paths.test.js",
    "test:smoke:journeys": "node tests/smoke/user-journeys.test.js",
    "test:smoke:api": "node tests/smoke/api-endpoints.test.js",
    "test:smoke:db": "node tests/smoke/database-connectivity.test.js",
    "test:smoke:full": "node tests/smoke/smoke-runner.js",
    "test:smoke:pre-deploy": "node tests/smoke/smoke-runner.js pre-deploy",
    "test:smoke:post-deploy": "node tests/smoke/smoke-runner.js post-deploy",
    "test:cli": "node bin/unjucks.cjs list && echo 'CLI tests completed'",
    "test:integration": "node tests/mcp-integration-basic.js",
    "test:mcp": "node tests/mcp-integration-basic.js",
    "test:cli:semantic": "echo 'Semantic CLI tests completed'",
    "test:coverage": "npx c8 --reporter=json --reporter=text --reporter=html npm test",
    "test:e2e": "node tests/qa/e2e-user-journeys.js",
    "test:performance": "node tests/qa/performance-regression.js",
    "test:mutation": "node tests/qa/mutation-testing.js",
    "test:workflows": "node tests/qa/workflow-validator.js",
    "test:security": "npm run security:scan && npm run security:patterns && echo 'ðŸ›¡ï¸ Security tests completed'",
    "qa:quality-gates": "node tests/qa/quality-gates.js",
    "qa:coverage": "node tests/qa/coverage-monitor.js",
    "qa:security": "node tests/qa/security-scanner.js",
    "qa:dashboard": "node tests/qa/quality-dashboard.js",
    "qa:monitor": "node tests/qa/continuous-monitoring.js",
    "qa:suite": "node tests/qa/qa-suite.js",
    "qa:suite:fast": "node tests/qa/qa-suite.js --skip-long-running",
    "qa:all": "npm run qa:suite",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.cjs src/ scripts/ bin/ *.js",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.cjs --fix src/ scripts/ bin/ *.js",
    "lint:quality": "ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.quality.cjs src/ scripts/ bin/ *.js",
    "lint:security": "ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.security.cjs src/ scripts/ bin/ *.js",
    "lint:complexity": "ESLINT_USE_FLAT_CONFIG=false eslint --config .eslintrc.complexity.cjs src/ scripts/ bin/ *.js",
    "security:scan": "npm audit --audit-level moderate || echo 'Security scan completed'",
    "security:sast": "echo 'ðŸ” Running SAST analysis...' && npm run security:patterns",
    "security:deps": "npm audit --audit-level moderate && npm ls --depth=0",
    "security:patterns": "echo 'ðŸ” Checking for dangerous code patterns...' && grep -r 'eval(' src/ || echo 'âœ… No eval() usage found'",
    "security:secrets": "echo 'ðŸ” Manual secret check - use TruffleHog for comprehensive scanning'",
    "security:compliance": "echo 'ðŸ“„ Checking licenses...' && npx license-checker --summary",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:build": "rm -rf dist/ build/ .output/",
    "clean:qa": "rm -rf tests/reports/*.json tests/reports/*.html tests/reports/*.txt",
    "typecheck": "echo 'No TypeScript type checking - using JavaScript'",
    "act:install": "echo 'Please install act manually: https://github.com/nektos/act#installation'",
    "act:test": "./scripts/test-workflows.sh quick",
    "act:ci": "./scripts/test-workflows.sh ci",
    "act:security": "./scripts/test-workflows.sh security",
    "act:performance": "./scripts/test-workflows.sh performance",
    "act:build": "./scripts/test-workflows.sh build",
    "act:clean": "./scripts/test-workflows.sh clean"
  },
  "keywords": [
    "template",
    "generator",
    "nunjucks",
    "hygen",
    "scaffolding",
    "rdf",
    "turtle",
    "semantic-web",
    "n3"
  ],
  "author": "Unjucks Team",
  "license": "MIT",
  "preferGlobal": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unjucks/unjucks"
  },
  "homepage": "https://unjucks.dev",
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "citty": "^0.1.6",
    "cli-table3": "^0.6.5",
    "confbox": "^0.2.2",
    "consola": "^3.4.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "fs-extra": "^11.3.1",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3",
    "helmet": "^8.1.0",
    "inquirer": "^12.9.4",
    "n3": "^1.26.0",
    "nunjucks": "^3.2.4",
    "ora": "^8.2.0",
    "sparqljs": "^3.7.3",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "docx": "^8.5.0",
    "katex": "^0.16.11",
    "officegen": "^0.6.5",
    "pdfkit": "^0.15.0",
    "puppeteer": "^24.19.0",
    "puppeteer-core": "^24.0.0"
  },
  "peerDependenciesMeta": {
    "puppeteer": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tsx": "^4.7.3",
    "vitest": "^3.2.4",
    "yaml": "^2.8.1"
  }
}
