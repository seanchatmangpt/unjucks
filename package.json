{
  "name": "kgen",
  "version": "0.1.0",
  "description": "Knowledge Graph Engine for deterministic artifact generation from RDF graphs",
  "type": "module",
  "private": true,
  "bin": {
    "kgen": "./dist/cli-entry.mjs"
  },
  "scripts": {
    "build": "npm run build:packages && unbuild",
    "build:dev": "npm run build:packages && unbuild --dev",
    "build:watch": "unbuild --watch",
    "build:analyze": "unbuild --analyze",
    "build:packages": "cd packages/kgen-core && npm run build && cd ../kgen-cli && npm run build && cd ../kgen-rules && npm run build && cd ../..",
    "build:clean": "rm -rf dist packages/*/dist",
    "test": "npm run test:cli && npm run test:imports && npm run test:v1:quick",
    "test:cas": "cucumber-js --config features/cucumber.config.ts",
    "test:cas:smoke": "cucumber-js --config features/cucumber.config.ts --tags '@core or @cas'",
    "test:cas:performance": "cucumber-js --config features/cucumber.config.ts --tags '@performance'",
    "test:cas:deterministic": "cucumber-js --config features/cucumber.config.ts --tags '@deterministic'",
    "test:cli": "node dist/cli-entry.mjs --version && node dist/cli-entry.mjs --help",
    "test:imports": "node -e \"['c12','citty','consola','n3','sparqljs','nunjucks','yaml','gray-matter','fs-extra'].forEach(d=>{try{require(d);console.log('✅',d)}catch(e){console.error('❌',d,e.message);process.exit(1)}})\"",
    "test:v1:quick": "node tests/run-v1-tests.js quick",
    "test:v1:full": "node tests/run-v1-tests.js full",
    "test:v1:deterministic": "npx cucumber-js --config tests/cucumber.config.js --profile deterministic",
    "test:v1:cas": "npx cucumber-js --config tests/cucumber.config.js --profile cas",
    "test:v1:attestation": "npx cucumber-js --config tests/cucumber.config.js --profile attestation",
    "test:v1:marketplace": "npx cucumber-js --config tests/cucumber.config.js --profile marketplace",
    "test:v1:shacl": "npx cucumber-js --config tests/cucumber.config.js --profile shacl",
    "test:v1:git-receipts": "npx cucumber-js --config tests/cucumber.config.js --profile git-receipts",
    "test:v1:personas": "npx cucumber-js --config tests/cucumber.config.js --profile personas",
    "test:v1:fuzz": "npx cucumber-js --config tests/cucumber.config.js --profile fuzz",
    "test:v1:performance": "npx cucumber-js --config tests/cucumber.config.js --profile performance",
    "test:v1:kpi": "npx cucumber-js --config tests/cucumber.config.js --profile kpi",
    "test:watch": "echo 'Watch mode not applicable for CLI tool'",
    "test:coverage": "echo 'Coverage not configured for CLI tool'",
    "lint": "eslint bin/ src/ --ext .js,.mjs --ignore-pattern '*.min.js'",
    "lint:check": "eslint bin/ src/ --ext .js,.mjs --ignore-pattern '*.min.js' --quiet",
    "lint:fix": "eslint bin/ src/ --ext .js,.mjs --fix --ignore-pattern '*.min.js'",
    "format": "prettier --write \"**/*.{js,mjs,json,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,mjs,json,md}\" --ignore-path .gitignore",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "typecheck": "echo 'TypeScript removed - using unbuild for type checking'",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "clean:artifacts": "rm -rf dist/ build/ coverage/ .nyc_output/",
    "validate": "npm run test:imports && npm run test:cli",
    "validate:full": "npm run validate && npm run security:audit",
    "dev": "unbuild --watch && node --watch dist/cli-entry.mjs",
    "dev:debug": "unbuild --watch && node --watch --inspect dist/cli-entry.mjs",
    "dev:unbuild": "unbuild --dev --watch",
    "start": "node dist/cli-entry.mjs",
    "start:prod": "NODE_ENV=production node dist/cli-entry.mjs",
    "kgen": "./dist/cli-entry.mjs",
    "preinstall": "echo 'Installing KGEN CLI dependencies...'",
    "postinstall": "echo 'KGEN CLI installation complete. Run: npm run validate'",
    "prepare": "npm run validate || echo 'Validation failed - check setup'",
    "docker:build": "docker build -f docker/Dockerfile.production -t kgen:latest .",
    "docker:run": "docker run --rm kgen:latest --version",
    "docker:test": "docker run --rm kgen:latest --help"
  },
  "dependencies": {
    "@noble/curves": "^2.0.0",
    "@noble/ed25519": "^3.0.0",
    "@noble/secp256k1": "^3.0.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.45.1",
    "@opentelemetry/sdk-trace-node": "^1.30.1",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@rdfjs/data-model": "^2.1.1",
    "@rdfjs/dataset": "^2.0.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "c12": "^1.11.2",
    "chalk": "^5.6.2",
    "citty": "^0.1.5",
    "clownface": "^2.0.3",
    "consola": "^3.4.2",
    "crypto-js": "^4.2.0",
    "fflate": "^0.8.2",
    "fs-extra": "^11.3.1",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "hash-wasm": "^4.12.0",
    "isomorphic-dompurify": "^2.26.0",
    "isomorphic-git": "^1.25.10",
    "jose": "^6.1.0",
    "jsondiffpatch": "^0.7.3",
    "multiformats": "^13.4.0",
    "n3": "^1.26.0",
    "node-forge": "^1.3.1",
    "nunjucks": "^3.2.4",
    "ora": "^9.0.0",
    "rdf-ext": "^2.6.0",
    "rdf-validate-shacl": "^0.6.5",
    "shacl-engine": "^1.0.2",
    "simple-git": "^3.20.0",
    "sparqljs": "^3.7.3",
    "table": "^6.9.0",
    "validator": "^13.15.15",
    "vm2": "^3.9.19",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "bcryptjs": "^3.0.2",
    "chai": "^6.0.1",
    "eslint": "^8.56.0",
    "jsonwebtoken": "^9.0.2",
    "prettier": "^3.6.2",
    "unbuild": "^2.0.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/kgen.git"
  },
  "keywords": [
    "knowledge-graph",
    "rdf",
    "code-generation",
    "provenance",
    "deterministic",
    "cli"
  ],
  "license": "MIT"
}
