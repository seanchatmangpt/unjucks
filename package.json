{
  "name": "@seanchatmangpt/unjucks",
  "version": "3.0.0",
  "type": "module",
  "description": "Next-generation template scaffolding with semantic web capabilities",
  "main": "./src/index.js",
  "bin": {
    "unjucks": "./bin/unjucks.js"
  },
  "exports": {
    ".": {
      "import": "./src/index.js"
    }
  },
  "files": [
    "src",
    "bin",
    "_templates",
    "README.md",
    "LICENSE"
  ],
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "build": "chmod +x bin/unjucks.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "dev": "node --watch bin/unjucks.js",
    "start": "node bin/unjucks.js"
  },
  "keywords": [
    "template",
    "generator",
    "nunjucks",
    "hygen",
    "scaffolding",
    "rdf",
    "turtle",
    "semantic-web",
    "n3"
  ],
  "author": "Unjucks Team",
  "license": "MIT",
  "preferGlobal": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unjucks/unjucks"
  },
  "homepage": "https://unjucks.dev",
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "c12": "^3.2.0",
    "chalk": "^5.3.0",
    "citty": "^0.1.6",
    "confbox": "^0.2.2",
    "consola": "^3.4.2",
    "dayjs": "^1.11.18",
    "fs-extra": "^11.3.1",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "inquirer": "^12.9.4",
    "n3": "^1.26.0",
    "nunjucks": "^3.2.4",
    "ora": "^8.2.0",
    "yaml": "^2.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "claude-flow": "^2.0.0-alpha.108",
    "eslint": "^9.0.0",
    "vitest": "^3.2.4"
  }
}
