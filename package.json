{
  "name": "kgen",
  "version": "0.1.0",
  "description": "Knowledge Graph Engine for deterministic artifact generation from RDF graphs",
  "type": "module",
  "private": true,
  "bin": {
    "kgen": "./bin/kgen.mjs"
  },
  "scripts": {
    "build": "echo 'No build step needed for main kgen CLI'",
    "test": "echo 'Tests not configured'",
    "lint": "eslint bin/ src/ --ext .js,.mjs,.ts",
    "typecheck": "echo 'TypeScript check not configured'",
    "clean": "echo 'No cleanup needed'",
    "dev": "node --watch bin/kgen.mjs",
    "kgen": "./bin/kgen.mjs"
  },
  "dependencies": {
    "c12": "^1.11.2",
    "citty": "^0.1.5",
    "consola": "^3.4.2",
