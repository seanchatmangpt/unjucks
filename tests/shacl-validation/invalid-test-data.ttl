ex:complexInvalid2 a ex:RangeTest, ex:PatternTest ;
    ex:percentage "200.0"^^xsd:decimal ; # Above max range
    ex:phoneNumber "invalid-phone" ;    # Invalid pattern
    ex:temperature "-500.0"^^xsd:decimal ; # Below absolute zero
    ex:hexColor "not-hex-color" .       # Invalid hex pattern
# VIOLATIONS: Multiple violations in range and pattern constraints