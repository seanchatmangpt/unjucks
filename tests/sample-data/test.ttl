# Test RDF Turtle file for semantic command testing
@prefix ex: <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:alice rdf:type foaf:Person ;
    foaf:name "Alice Smith" ;
    foaf:age "30"^^<http://www.w3.org/2001/XMLSchema#integer> ;
    foaf:knows ex:bob .

ex:bob rdf:type foaf:Person ;
    foaf:name "Bob Johnson" ;
    foaf:mbox "bob@example.com" .

ex:Company rdf:type rdfs:Class ;
    rdfs:label "Company" .

ex:acme rdf:type ex:Company ;
    rdfs:label "ACME Corporation" .