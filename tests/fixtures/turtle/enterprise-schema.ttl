    rdfs:label "Platform Engineering" ;
    dc:description "Shared platform services and infrastructure" ;
    enterprise:parentOrganization enterprise:TechnologyDivision .

enterprise:ProductEngineering a enterprise:Department ;
    rdfs:label "Product Engineering" ;
    dc:description "Customer-facing product development" ;
    enterprise:parentOrganization enterprise:TechnologyDivision .

enterprise:SecurityEngineering a enterprise:Department ;
    rdfs:label "Security Engineering" ;
    dc:description "Enterprise security and compliance" ;
    enterprise:parentOrganization enterprise:TechnologyDivision .

