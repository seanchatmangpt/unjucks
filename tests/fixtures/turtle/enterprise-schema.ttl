@prefix enterprise: <http://example.org/enterprise#> .
@prefix api: <http://example.org/api#> .
@prefix security: <http://example.org/security#> .
@prefix compliance: <http://example.org/compliance#> .
@prefix governance: <http://example.org/governance#> .
@prefix team: <http://example.org/team#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .

# Enterprise Architecture Ontology
# This ontology defines the core concepts, relationships, and constraints
# for Fortune 5 enterprise API governance and compliance

# ============================
# CLASS DEFINITIONS
# ============================

# Core Enterprise Classes
enterprise:Organization a owl:Class ;
    rdfs:label "Enterprise Organization" ;
    rdfs:comment "Top-level organizational entity with governance authority" .

enterprise:BusinessUnit rdfs:subClassOf enterprise:Organization ;
    rdfs:label "Business Unit" ;
    rdfs:comment "Autonomous business division with P&L responsibility" .

enterprise:Department rdfs:subClassOf enterprise:Organization ;
    rdfs:label "Department" ;
    rdfs:comment "Functional organizational unit within a business unit" .

# Service Architecture Classes
api:Service a owl:Class ;
    rdfs:label "Enterprise Service" ;
    rdfs:comment "Reusable business capability exposed through well-defined interfaces" .

api:APIService rdfs:subClassOf api:Service ;
    rdfs:label "API Service" ;
    rdfs:comment "Service exposed through Application Programming Interface" .

api:RESTService rdfs:subClassOf api:APIService ;
    rdfs:label "REST API Service" ;
    rdfs:comment "RESTful web service following REST architectural constraints" .

api:GraphQLService rdfs:subClassOf api:APIService ;
    rdfs:label "GraphQL API Service" ;
    rdfs:comment "GraphQL-based service with flexible query capabilities" .

api:MicroService rdfs:subClassOf api:Service ;
    rdfs:label "Microservice" ;
    rdfs:comment "Small, autonomous service with single business responsibility" .

# Security Classifications
security:SecurityLevel a owl:Class ;
    rdfs:label "Security Level" ;
    rdfs:comment "Classification level for security requirements" .

security:Public rdfs:subClassOf security:SecurityLevel ;
    rdfs:label "Public Security Level" ;
    rdfs:comment "Information available to general public" .

security:Internal rdfs:subClassOf security:SecurityLevel ;
    rdfs:label "Internal Security Level" ;
    rdfs:comment "Information restricted to organization employees" .

security:Confidential rdfs:subClassOf security:SecurityLevel ;
    rdfs:label "Confidential Security Level" ;
    rdfs:comment "Sensitive information requiring special handling" .

security:Restricted rdfs:subClassOf security:SecurityLevel ;
    rdfs:label "Restricted Security Level" ;
    rdfs:comment "Highly sensitive information with strict access controls" .

# Compliance Framework Classes
compliance:ComplianceFramework a owl:Class ;
    rdfs:label "Compliance Framework" ;
    rdfs:comment "Regulatory or policy framework requiring adherence" .

compliance:SOX rdfs:subClassOf compliance:ComplianceFramework ;
    rdfs:label "Sarbanes-Oxley Act" ;
    rdfs:comment "Financial reporting and corporate governance compliance" ;
    compliance:jurisdiction "United States" ;
    compliance:scope "Financial Reporting" .

compliance:GDPR rdfs:subClassOf compliance:ComplianceFramework ;
    rdfs:label "General Data Protection Regulation" ;
    rdfs:comment "European Union data protection and privacy regulation" ;
    compliance:jurisdiction "European Union" ;
    compliance:scope "Personal Data Protection" .

compliance:HIPAA rdfs:subClassOf compliance:ComplianceFramework ;
    rdfs:label "Health Insurance Portability and Accountability Act" ;
    rdfs:comment "Healthcare data protection regulation" ;
    compliance:jurisdiction "United States" ;
    compliance:scope "Protected Health Information" .

compliance:PCI_DSS rdfs:subClassOf compliance:ComplianceFramework ;
    rdfs:label "Payment Card Industry Data Security Standard" ;
    rdfs:comment "Credit card data protection requirements" ;
    compliance:jurisdiction "Global" ;
    compliance:scope "Payment Card Data" .

# Governance Classes
governance:Policy a owl:Class ;
    rdfs:label "Governance Policy" ;
    rdfs:comment "Organizational rule or guideline for behavior and decisions" .

governance:SecurityPolicy rdfs:subClassOf governance:Policy ;
    rdfs:label "Security Policy" ;
    rdfs:comment "Policy governing security practices and requirements" .

governance:DataGovernancePolicy rdfs:subClassOf governance:Policy ;
    rdfs:label "Data Governance Policy" ;
    rdfs:comment "Policy governing data management and protection" .

governance:APIGovernancePolicy rdfs:subClassOf governance:Policy ;
    rdfs:label "API Governance Policy" ;
    rdfs:comment "Policy governing API design, development, and lifecycle" .

# Team and Ownership Classes
team:Team a owl:Class ;
    rdfs:label "Development Team" ;
    rdfs:comment "Group of individuals responsible for service development" .

team:ProductTeam rdfs:subClassOf team:Team ;
    rdfs:label "Product Team" ;
    rdfs:comment "Cross-functional team responsible for product development" .

team:PlatformTeam rdfs:subClassOf team:Team ;
    rdfs:label "Platform Team" ;
    rdfs:comment "Team providing shared platform capabilities" .

# ============================
# PROPERTY DEFINITIONS  
# ============================

# Service Properties
api:serviceName a owl:DatatypeProperty ;
    rdfs:label "Service Name" ;
    rdfs:comment "Human-readable name of the service" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:string .

api:serviceDescription a owl:DatatypeProperty ;
    rdfs:label "Service Description" ;
    rdfs:comment "Detailed description of service purpose and functionality" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:string .

api:version a owl:DatatypeProperty ;
    rdfs:label "Service Version" ;
    rdfs:comment "Version identifier for the service" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:string .

api:endpoint a owl:DatatypeProperty ;
    rdfs:label "Service Endpoint" ;
    rdfs:comment "Network location where service can be accessed" ;
    rdfs:domain api:APIService ;
    rdfs:range xsd:anyURI .

api:basePath a owl:DatatypeProperty ;
    rdfs:label "Base Path" ;
    rdfs:comment "Base URL path for API endpoints" ;
    rdfs:domain api:APIService ;
    rdfs:range xsd:string .

# Security Properties
security:securityLevel a owl:ObjectProperty ;
    rdfs:label "Security Level" ;
    rdfs:comment "Classification level for security requirements" ;
    rdfs:domain api:Service ;
    rdfs:range security:SecurityLevel .

security:requiresAuthentication a owl:DatatypeProperty ;
    rdfs:label "Requires Authentication" ;
    rdfs:comment "Indicates whether service requires user authentication" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:boolean .

security:requiresAuthorization a owl:DatatypeProperty ;
    rdfs:label "Requires Authorization" ;
    rdfs:comment "Indicates whether service requires user authorization" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:boolean .

security:encryptionRequired a owl:DatatypeProperty ;
    rdfs:label "Encryption Required" ;
    rdfs:comment "Indicates whether data must be encrypted" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:boolean .

# Compliance Properties
compliance:applicableFrameworks a owl:ObjectProperty ;
    rdfs:label "Applicable Compliance Frameworks" ;
    rdfs:comment "Compliance frameworks that apply to this service" ;
    rdfs:domain api:Service ;
    rdfs:range compliance:ComplianceFramework .

compliance:auditRequired a owl:DatatypeProperty ;
    rdfs:label "Audit Required" ;
    rdfs:comment "Indicates whether service activities must be audited" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:boolean .

compliance:dataClassification a owl:DatatypeProperty ;
    rdfs:label "Data Classification" ;
    rdfs:comment "Classification of data handled by the service" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:string .

compliance:retentionPeriod a owl:DatatypeProperty ;
    rdfs:label "Data Retention Period" ;
    rdfs:comment "Period for which data must be retained" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:duration .

# Governance Properties
governance:owner a owl:ObjectProperty ;
    rdfs:label "Service Owner" ;
    rdfs:comment "Team or individual responsible for service governance" ;
    rdfs:domain api:Service ;
    rdfs:range team:Team .

governance:approvalRequired a owl:DatatypeProperty ;
    rdfs:label "Approval Required" ;
    rdfs:comment "Indicates whether changes require formal approval" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:boolean .

governance:reviewCycle a owl:DatatypeProperty ;
    rdfs:label "Review Cycle" ;
    rdfs:comment "Frequency of mandatory governance reviews" ;
    rdfs:domain api:Service ;
    rdfs:range xsd:duration .

# Team Properties
team:teamName a owl:DatatypeProperty ;
    rdfs:label "Team Name" ;
    rdfs:comment "Name of the development team" ;
    rdfs:domain team:Team ;
    rdfs:range xsd:string .

team:teamLead a owl:DatatypeProperty ;
    rdfs:label "Team Lead" ;
    rdfs:comment "Individual leading the team" ;
    rdfs:domain team:Team ;
    rdfs:range xsd:string .

team:contactEmail a owl:DatatypeProperty ;
    rdfs:label "Contact Email" ;
    rdfs:comment "Primary contact email for the team" ;
    rdfs:domain team:Team ;
    rdfs:range xsd:string .

# ============================
# ENTERPRISE INSTANCES
# ============================

# Fortune 5 Company Structure
