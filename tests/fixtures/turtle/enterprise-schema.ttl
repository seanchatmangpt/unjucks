enterprise:TechnologyDivision a enterprise:BusinessUnit ;
    rdfs:label "Technology Division" ;
    dc:description "Core technology products and platforms" ;
    enterprise:parentOrganization enterprise:FortuneCorpGlobal ;
    enterprise:revenue "200000000000"^^xsd:decimal .

enterprise:FinancialServicesDivision a enterprise:BusinessUnit ;
    rdfs:label "Financial Services Division" ;
    dc:description "Banking, payments, and financial products" ;
    enterprise:parentOrganization enterprise:FortuneCorpGlobal ;
    enterprise:revenue "150000000000"^^xsd:decimal .

# Key Departments
