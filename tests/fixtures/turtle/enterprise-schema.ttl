# ============================
# CRITICAL ENTERPRISE SERVICES
# ============================

# User Management Service
api:UserManagementService a api:RESTService ;
    api:serviceName "User Management Service" ;
    api:serviceDescription "Central service for user identity, authentication, and profile management" ;
    api:version "2.1.0" ;
    api:endpoint <https://api.fortunecorp.com/users/v2> ;
    api:basePath "/users/v2" ;
    governance:owner team:UserPlatformTeam ;
    security:securityLevel security:Confidential ;
    security:requiresAuthentication true ;
    security:requiresAuthorization true ;
    security:encryptionRequired true ;
    compliance:applicableFrameworks compliance:GDPR, compliance:SOX ;
    compliance:auditRequired true ;
    compliance:dataClassification "Personal Identifiable Information" ;
    compliance:retentionPeriod "P7Y"^^xsd:duration ;
    governance:approvalRequired true ;
    governance:reviewCycle "P3M"^^xsd:duration ;
    api:rateLimit "1000"^^xsd:integer ;
    api:availabilitySLA "99.9"^^xsd:decimal .

# Payment Processing Service
api:PaymentProcessingService a api:RESTService ;
    api:serviceName "Payment Processing Service" ;
    api:serviceDescription "Secure payment processing with multi-provider support" ;
    api:version "3.2.1" ;
    api:endpoint <https://api.fortunecorp.com/payments/v3> ;
    api:basePath "/payments/v3" ;
    governance:owner team:PaymentPlatformTeam ;
    security:securityLevel security:Restricted ;
    security:requiresAuthentication true ;
    security:requiresAuthorization true ;
    security:encryptionRequired true ;
    compliance:applicableFrameworks compliance:PCI_DSS, compliance:SOX ;
    compliance:auditRequired true ;
    compliance:dataClassification "Financial Data" ;
    compliance:retentionPeriod "P10Y"^^xsd:duration ;
    governance:approvalRequired true ;
    governance:reviewCycle "P1M"^^xsd:duration ;
    api:rateLimit "500"^^xsd:integer ;
    api:availabilitySLA "99.99"^^xsd:decimal .

# Analytics Data Service
api:AnalyticsDataService a api:GraphQLService ;
    api:serviceName "Analytics Data Service" ;
    api:serviceDescription "Flexible analytics data access with GraphQL queries" ;
    api:version "1.5.0" ;
    api:endpoint <https://api.fortunecorp.com/analytics/graphql> ;
    api:basePath "/analytics/graphql" ;
    governance:owner team:UserPlatformTeam ;
    security:securityLevel security:Internal ;
    security:requiresAuthentication true ;
    security:requiresAuthorization true ;
    security:encryptionRequired true ;
    compliance:applicableFrameworks compliance:GDPR ;
    compliance:auditRequired false ;
    compliance:dataClassification "Aggregated Analytics Data" ;
    compliance:retentionPeriod "P5Y"^^xsd:duration ;
    governance:approvalRequired false ;
    governance:reviewCycle "P6M"^^xsd:duration ;
    api:rateLimit "2000"^^xsd:integer ;
    api:availabilitySLA "99.5"^^xsd:decimal .

# ============================
# GOVERNANCE POLICIES
# ============================

governance:EnterpriseAPIPolicy a governance:APIGovernancePolicy ;
    rdfs:label "Enterprise API Governance Policy" ;
    dc:description "Comprehensive policy governing all API development and lifecycle" ;
    governance:version "2.0" ;
    governance:effectiveDate "2024-01-01"^^xsd:date ;
    governance:mandatoryCompliance true ;
    governance:applicableServices api:Service .

governance:SecurityByDesignPolicy a governance:SecurityPolicy ;
    rdfs:label "Security by Design Policy" ;
    dc:description "Security requirements must be integrated from design phase" ;
    governance:version "1.3" ;
    governance:effectiveDate "2023-06-01"^^xsd:date ;
    governance:mandatoryCompliance true ;
    governance:requires security:requiresAuthentication, 
                        security:requiresAuthorization,
                        security:encryptionRequired .

governance:DataGovernancePolicy a governance:DataGovernancePolicy ;
    rdfs:label "Enterprise Data Governance Policy" ;
    dc:description "Policy governing data classification, handling, and protection" ;
    governance:version "1.8" ;
    governance:effectiveDate "2023-09-01"^^xsd:date ;
    governance:mandatoryCompliance true ;
    governance:requires compliance:dataClassification,
                        compliance:retentionPeriod,
                        compliance:auditRequired .

# ============================
# COMPLIANCE CONSTRAINTS
# ============================

# SOX Compliance Constraints
compliance:SOXConstraints a owl:Class ;
    rdfs:label "SOX Compliance Constraints" ;
    rdfs:comment "Constraints required for SOX compliance" .

compliance:SOXFinancialService owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
        api:Service
        [a owl:Restriction ;
         owl:onProperty compliance:dataClassification ;
         owl:hasValue "Financial Data"]
    )
] .

# GDPR Compliance Constraints  
compliance:GDPRConstraints a owl:Class ;
    rdfs:label "GDPR Compliance Constraints" ;
    rdfs:comment "Constraints required for GDPR compliance" .

compliance:GDPRPersonalDataService owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
        api:Service
        [a owl:Restriction ;
         owl:onProperty compliance:dataClassification ;
         owl:hasValue "Personal Identifiable Information"]
    )
] .

# PCI-DSS Compliance Constraints
compliance:PCIConstraints a owl:Class ;
    rdfs:label "PCI-DSS Compliance Constraints" ;
    rdfs:comment "Constraints required for PCI-DSS compliance" .

compliance:PCIPaymentService owl:equivalentClass [
    a owl:Class ;
    owl:intersectionOf (
        api:Service
        [a owl:Restriction ;
         owl:onProperty compliance:dataClassification ;
         owl:hasValue "Payment Card Data"]
    )
] .