team:PaymentPlatformTeam a team:PlatformTeam ;
    team:teamName "Payment Platform Team" ;
    team:teamLead "Michael Chen" ;
    team:contactEmail "payments@fortunecorp.com" ;
    team:parentDepartment enterprise:PlatformEngineering ;
    team:teamSize "15"^^xsd:integer ;
