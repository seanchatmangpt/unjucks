    gs1:gtin "00012345678906" ;
    dcterms:title "Another Test Pharmaceutical - 200mg Capsules" ;
    gs1:batchNumber "BATCH-2024-002" ;
    gs1:serialNumber "SN-002-789456124" ;
    blockchain:hashValue "0xb1c2d3e4f5a6789012345678901234567890abcdef1234567890abcdef234567" ;
    blockchain:blockchainType "ethereum" .

<product/pharmaceutical-003> a gs1:TradeItem ;
    gs1:gtin "00012345678907" ;
    dcterms:title "Third Test Pharmaceutical - 50mg Liquid" ;
    gs1:batchNumber "BATCH-2024-003" ;
    gs1:serialNumber "SN-003-789456125" ;
    blockchain:hashValue "0xc1d2e3f4a5b6789012345678901234567890abcdef1234567890abcdef345678" ;
    blockchain:blockchainType "hyperledger" .