{\n  \"dashboards\": [\n    {\n      \"id\": \"unjucks-overview\",\n      \"title\": \"Unjucks Service Overview\",\n      \"description\": \"High-level overview of service health and performance\",\n      \"dashboard\": {\n        \"id\": null,\n        \"title\": \"Unjucks Service Overview\",\n        \"tags\": [\"unjucks\", \"overview\"],\n        \"timezone\": \"browser\",\n        \"refresh\": \"30s\",\n        \"time\": {\n          \"from\": \"now-1h\",\n          \"to\": \"now\"\n        },\n        \"panels\": [\n          {\n            \"id\": 1,\n            \"title\": \"Service Health Status\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"up{job=\\\"unjucks-service\\\"}\",\n                \"legendFormat\": \"Service Status\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"red\", \"value\": 0 },\n                    { \"color\": \"green\", \"value\": 1 }\n                  ]\n                },\n                \"mappings\": [\n                  { \"options\": { \"0\": { \"text\": \"DOWN\" } } },\n                  { \"options\": { \"1\": { \"text\": \"UP\" } } }\n                ]\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 0, \"y\": 0 }\n          },\n          {\n            \"id\": 2,\n            \"title\": \"Request Rate\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_http_requests_total[5m])\",\n                \"legendFormat\": \"Requests/sec\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"reqps\",\n                \"decimals\": 2\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 6, \"y\": 0 }\n          },\n          {\n            \"id\": 3,\n            \"title\": \"Error Rate\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_http_requests_total{status_code=~\\\"4..|5..\\\"} [5m]) / rate(unjucks_http_requests_total[5m]) * 100\",\n                \"legendFormat\": \"Error Rate %\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"percent\",\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"green\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 1 },\n                    { \"color\": \"red\", \"value\": 5 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 12, \"y\": 0 }\n          },\n          {\n            \"id\": 4,\n            \"title\": \"Response Time P95\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"histogram_quantile(0.95, rate(unjucks_http_request_duration_seconds_bucket[5m]))\",\n                \"legendFormat\": \"P95 Response Time\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"s\",\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"green\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 0.5 },\n                    { \"color\": \"red\", \"value\": 2 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 18, \"y\": 0 }\n          },\n          {\n            \"id\": 5,\n            \"title\": \"HTTP Request Rate by Method\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_http_requests_total[5m])\",\n                \"legendFormat\": \"{{method}} {{route}}\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Requests/sec\",\n                \"min\": 0\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 0, \"y\": 4 }\n          },\n          {\n            \"id\": 6,\n            \"title\": \"Response Time Distribution\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"histogram_quantile(0.50, rate(unjucks_http_request_duration_seconds_bucket[5m]))\",\n                \"legendFormat\": \"P50\"\n              },\n              {\n                \"expr\": \"histogram_quantile(0.95, rate(unjucks_http_request_duration_seconds_bucket[5m]))\",\n                \"legendFormat\": \"P95\"\n              },\n              {\n                \"expr\": \"histogram_quantile(0.99, rate(unjucks_http_request_duration_seconds_bucket[5m]))\",\n                \"legendFormat\": \"P99\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Response Time (seconds)\",\n                \"min\": 0\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 12, \"y\": 4 }\n          },\n          {\n            \"id\": 7,\n            \"title\": \"Memory Usage\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"unjucks_memory_usage_bytes{type=\\\"heap_used\\\"}\",\n                \"legendFormat\": \"Heap Used\"\n              },\n              {\n                \"expr\": \"unjucks_memory_usage_bytes{type=\\\"heap_total\\\"}\",\n                \"legendFormat\": \"Heap Total\"\n              },\n              {\n                \"expr\": \"unjucks_memory_usage_bytes{type=\\\"rss\\\"}\",\n                \"legendFormat\": \"RSS\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Memory (bytes)\",\n                \"min\": 0\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 0, \"y\": 12 }\n          },\n          {\n            \"id\": 8,\n            \"title\": \"CPU Usage\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_cpu_usage_seconds_total[5m]) * 100\",\n                \"legendFormat\": \"CPU Usage %\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"CPU Usage (%)\",\n                \"min\": 0,\n                \"max\": 100\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 12, \"y\": 12 }\n          }\n        ]\n      }\n    },\n    {\n      \"id\": \"unjucks-slo\",\n      \"title\": \"Unjucks SLI/SLO Dashboard\",\n      \"description\": \"Service Level Indicators and Objectives monitoring\",\n      \"dashboard\": {\n        \"id\": null,\n        \"title\": \"Unjucks SLI/SLO Dashboard\",\n        \"tags\": [\"unjucks\", \"slo\", \"sli\"],\n        \"timezone\": \"browser\",\n        \"refresh\": \"1m\",\n        \"time\": {\n          \"from\": \"now-24h\",\n          \"to\": \"now\"\n        },\n        \"panels\": [\n          {\n            \"id\": 1,\n            \"title\": \"Availability SLO (99.9%)\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"((sum(rate(unjucks_http_requests_total{status_code!~\\\"5..\\\"} [24h])) / sum(rate(unjucks_http_requests_total[24h]))) * 100)\",\n                \"legendFormat\": \"Availability %\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"percent\",\n                \"decimals\": 3,\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"red\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 99.5 },\n                    { \"color\": \"green\", \"value\": 99.9 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 0, \"y\": 0 }\n          },\n          {\n            \"id\": 2,\n            \"title\": \"Latency SLO (95% < 500ms)\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"(histogram_quantile(0.95, rate(unjucks_http_request_duration_seconds_bucket[24h])) < 0.5) * 100\",\n                \"legendFormat\": \"Latency SLO %\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"percent\",\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"red\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 90 },\n                    { \"color\": \"green\", \"value\": 95 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 6, \"y\": 0 }\n          },\n          {\n            \"id\": 3,\n            \"title\": \"Error Budget Remaining\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"(1 - (rate(unjucks_http_requests_total{status_code=~\\\"5..\\\"} [24h]) / rate(unjucks_http_requests_total[24h])) / 0.001) * 100\",\n                \"legendFormat\": \"Error Budget %\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"percent\",\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"red\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 10 },\n                    { \"color\": \"green\", \"value\": 50 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 12, \"y\": 0 }\n          },\n          {\n            \"id\": 4,\n            \"title\": \"SLO Compliance Score\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"((unjucks_slo_availability_compliant + unjucks_slo_latency_compliant + unjucks_slo_error_rate_compliant) / 3) * 100\",\n                \"legendFormat\": \"Overall SLO Score\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"percent\",\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"red\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 80 },\n                    { \"color\": \"green\", \"value\": 95 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 18, \"y\": 0 }\n          },\n          {\n            \"id\": 5,\n            \"title\": \"Availability SLI Trend\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"((sum(rate(unjucks_http_requests_total{status_code!~\\\"5..\\\"} [1h])) / sum(rate(unjucks_http_requests_total[1h]))) * 100)\",\n                \"legendFormat\": \"Availability %\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Availability (%)\",\n                \"min\": 99,\n                \"max\": 100\n              }\n            ],\n            \"thresholds\": [\n              {\n                \"value\": 99.9,\n                \"colorMode\": \"critical\",\n                \"op\": \"lt\"\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 0, \"y\": 4 }\n          },\n          {\n            \"id\": 6,\n            \"title\": \"Error Budget Burn Rate\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"(rate(unjucks_http_requests_total{status_code=~\\\"5..\\\"} [1h]) / rate(unjucks_http_requests_total[1h])) / 0.001\",\n                \"legendFormat\": \"1h Burn Rate\"\n              },\n              {\n                \"expr\": \"(rate(unjucks_http_requests_total{status_code=~\\\"5..\\\"} [6h]) / rate(unjucks_http_requests_total[6h])) / 0.001\",\n                \"legendFormat\": \"6h Burn Rate\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Burn Rate (x target)\",\n                \"min\": 0\n              }\n            ],\n            \"thresholds\": [\n              {\n                \"value\": 14.4,\n                \"colorMode\": \"critical\",\n                \"op\": \"gt\"\n              },\n              {\n                \"value\": 6,\n                \"colorMode\": \"warn\",\n                \"op\": \"gt\"\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 12, \"y\": 4 }\n          }\n        ]\n      }\n    },\n    {\n      \"id\": \"unjucks-alerts\",\n      \"title\": \"Unjucks Alerts Dashboard\",\n      \"description\": \"Alert status and incident tracking\",\n      \"dashboard\": {\n        \"id\": null,\n        \"title\": \"Unjucks Alerts Dashboard\",\n        \"tags\": [\"unjucks\", \"alerts\"],\n        \"timezone\": \"browser\",\n        \"refresh\": \"30s\",\n        \"time\": {\n          \"from\": \"now-6h\",\n          \"to\": \"now\"\n        },\n        \"panels\": [\n          {\n            \"id\": 1,\n            \"title\": \"Active Alerts\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"sum(ALERTS{alertstate=\\\"firing\\\", service=\\\"unjucks\\\"})\",\n                \"legendFormat\": \"Active Alerts\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"green\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 1 },\n                    { \"color\": \"red\", \"value\": 5 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 0, \"y\": 0 }\n          },\n          {\n            \"id\": 2,\n            \"title\": \"Critical Alerts\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"sum(ALERTS{alertstate=\\\"firing\\\", service=\\\"unjucks\\\", severity=\\\"critical\\\"})\",\n                \"legendFormat\": \"Critical Alerts\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"green\", \"value\": 0 },\n                    { \"color\": \"red\", \"value\": 1 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 6, \"y\": 0 }\n          },\n          {\n            \"id\": 3,\n            \"title\": \"MTTR (Mean Time to Recovery)\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"avg(time() - unjucks_alert_start_time{status=\\\"resolved\\\"})\",\n                \"legendFormat\": \"MTTR (seconds)\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"s\"\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 12, \"y\": 0 }\n          },\n          {\n            \"id\": 4,\n            \"title\": \"Alert Frequency\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_alerts_total[24h])\",\n                \"legendFormat\": \"Alerts/hour\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"short\",\n                \"decimals\": 2\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 18, \"y\": 0 }\n          },\n          {\n            \"id\": 5,\n            \"title\": \"Alert Timeline\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"sum by (severity) (ALERTS{alertstate=\\\"firing\\\", service=\\\"unjucks\\\"})\",\n                \"legendFormat\": \"{{severity}}\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Number of Alerts\",\n                \"min\": 0\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 24, \"x\": 0, \"y\": 4 }\n          },\n          {\n            \"id\": 6,\n            \"title\": \"Alert Details\",\n            \"type\": \"table\",\n            \"targets\": [\n              {\n                \"expr\": \"ALERTS{alertstate=\\\"firing\\\", service=\\\"unjucks\\\"}\",\n                \"format\": \"table\"\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 24, \"x\": 0, \"y\": 12 }\n          }\n        ]\n      }\n    },\n    {\n      \"id\": \"unjucks-business\",\n      \"title\": \"Unjucks Business Metrics\",\n      \"description\": \"Business KPIs and application-specific metrics\",\n      \"dashboard\": {\n        \"id\": null,\n        \"title\": \"Unjucks Business Metrics\",\n        \"tags\": [\"unjucks\", \"business\"],\n        \"timezone\": \"browser\",\n        \"refresh\": \"5m\",\n        \"time\": {\n          \"from\": \"now-24h\",\n          \"to\": \"now\"\n        },\n        \"panels\": [\n          {\n            \"id\": 1,\n            \"title\": \"File Generations per Hour\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_file_generations_total[1h]) * 3600\",\n                \"legendFormat\": \"Files/hour\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"short\",\n                \"decimals\": 0\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 0, \"y\": 0 }\n          },\n          {\n            \"id\": 2,\n            \"title\": \"Template Renders per Hour\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_template_renders_total[1h]) * 3600\",\n                \"legendFormat\": \"Renders/hour\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"short\",\n                \"decimals\": 0\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 6, \"y\": 0 }\n          },\n          {\n            \"id\": 3,\n            \"title\": \"API Calls Success Rate\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"(rate(unjucks_api_calls_total{status=\\\"success\\\"}[1h]) / rate(unjucks_api_calls_total[1h])) * 100\",\n                \"legendFormat\": \"Success Rate %\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"percent\",\n                \"thresholds\": {\n                  \"steps\": [\n                    { \"color\": \"red\", \"value\": 0 },\n                    { \"color\": \"yellow\", \"value\": 95 },\n                    { \"color\": \"green\", \"value\": 99 }\n                  ]\n                }\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 12, \"y\": 0 }\n          },\n          {\n            \"id\": 4,\n            \"title\": \"User Registrations per Day\",\n            \"type\": \"stat\",\n            \"targets\": [\n              {\n                \"expr\": \"increase(unjucks_user_registrations_total[24h])\",\n                \"legendFormat\": \"Registrations/day\"\n              }\n            ],\n            \"fieldConfig\": {\n              \"defaults\": {\n                \"unit\": \"short\",\n                \"decimals\": 0\n              }\n            },\n            \"gridPos\": { \"h\": 4, \"w\": 6, \"x\": 18, \"y\": 0 }\n          },\n          {\n            \"id\": 5,\n            \"title\": \"File Generation Trends\",\n            \"type\": \"graph\",\n            \"targets\": [\n              {\n                \"expr\": \"rate(unjucks_file_generations_total[5m])\",\n                \"legendFormat\": \"Generation Rate\"\n              }\n            ],\n            \"yAxes\": [\n              {\n                \"label\": \"Generations/sec\",\n                \"min\": 0\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 0, \"y\": 4 }\n          },\n          {\n            \"id\": 6,\n            \"title\": \"Template Usage by Type\",\n            \"type\": \"piechart\",\n            \"targets\": [\n              {\n                \"expr\": \"sum by (template_type) (rate(unjucks_template_renders_total[1h]))\",\n                \"legendFormat\": \"{{template_type}}\"\n              }\n            ],\n            \"gridPos\": { \"h\": 8, \"w\": 12, \"x\": 12, \"y\": 4 }\n          }\n        ]\n      }\n    }\n  ],\n  \"alertRules\": [\n    {\n      \"alert\": \"UnjucksServiceDown\",\n      \"expr\": \"up{job=\\\"unjucks-service\\\"} == 0\",\n      \"for\": \"1m\",\n      \"labels\": {\n        \"severity\": \"critical\",\n        \"service\": \"unjucks\"\n      },\n      \"annotations\": {\n        \"summary\": \"Unjucks service is down\",\n        \"description\": \"The Unjucks service has been down for more than 1 minute\"\n      }\n    },\n    {\n      \"alert\": \"UnjucksHighErrorRate\",\n      \"expr\": \"rate(unjucks_http_requests_total{status_code=~\\\"5..\\\"} [5m]) / rate(unjucks_http_requests_total[5m]) > 0.05\",\n      \"for\": \"5m\",\n      \"labels\": {\n        \"severity\": \"high\",\n        \"service\": \"unjucks\"\n      },\n      \"annotations\": {\n        \"summary\": \"High error rate detected\",\n        \"description\": \"Error rate is above 5% for the last 5 minutes\"\n      }\n    },\n    {\n      \"alert\": \"UnjucksHighLatency\",\n      \"expr\": \"histogram_quantile(0.95, rate(unjucks_http_request_duration_seconds_bucket[5m])) > 2\",\n      \"for\": \"10m\",\n      \"labels\": {\n        \"severity\": \"medium\",\n        \"service\": \"unjucks\"\n      },\n      \"annotations\": {\n        \"summary\": \"High response latency\",\n        \"description\": \"95th percentile latency is above 2 seconds for the last 10 minutes\"\n      }\n    },\n    {\n      \"alert\": \"UnjucksMemoryUsageHigh\",\n      \"expr\": \"unjucks_memory_usage_bytes{type=\\\"rss\\\"} / 1024 / 1024 / 1024 > 1\",\n      \"for\": \"10m\",\n      \"labels\": {\n        \"severity\": \"high\",\n        \"service\": \"unjucks\"\n      },\n      \"annotations\": {\n        \"summary\": \"High memory usage\",\n        \"description\": \"Memory usage is above 1GB for the last 10 minutes\"\n      }\n    },\n    {\n      \"alert\": \"UnjucksSLOViolation\",\n      \"expr\": \"((sum(rate(unjucks_http_requests_total{status_code!~\\\"5..\\\"} [24h])) / sum(rate(unjucks_http_requests_total[24h]))) * 100) < 99.9\",\n      \"for\": \"1m\",\n      \"labels\": {\n        \"severity\": \"critical\",\n        \"service\": \"unjucks\"\n      },\n      \"annotations\": {\n        \"summary\": \"SLO violation - Availability below 99.9%\",\n        \"description\": \"Service availability is below the 99.9% SLO target\"\n      }\n    },\n    {\n      \"alert\": \"UnjucksErrorBudgetDepleted\",\n      \"expr\": \"(1 - (rate(unjucks_http_requests_total{status_code=~\\\"5..\\\"} [1h]) / rate(unjucks_http_requests_total[1h])) / 0.001) < 0.1\",\n      \"for\": \"5m\",\n      \"labels\": {\n        \"severity\": \"critical\",\n        \"service\": \"unjucks\"\n      },\n      \"annotations\": {\n        \"summary\": \"Error budget nearly depleted\",\n        \"description\": \"Less than 10% of error budget remaining\"\n      }\n    }\n  ]\n}