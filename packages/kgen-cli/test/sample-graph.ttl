@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:ApiService a ex:Service ;
    rdfs:label "User API Service" ;
    ex:hasEndpoint ex:UserEndpoint ;
    ex:hasVersion "1.0.0"^^xsd:string ;
    ex:requiresAuth true .

ex:UserEndpoint a ex:Endpoint ;
    rdfs:label "User Management Endpoint" ;
    ex:hasPath "/api/users" ;
    ex:supportsMethod "GET", "POST", "PUT", "DELETE" ;
    ex:hasParameter ex:UserIdParam .

ex:UserIdParam a ex:Parameter ;
    rdfs:label "User ID Parameter" ;
    ex:hasName "userId" ;
    ex:hasType "string" ;
    ex:isRequired true .

ex:User a ex:Entity ;
    rdfs:label "User Entity" ;
    ex:hasField ex:NameField, ex:EmailField, ex:CreatedField .

ex:NameField a ex:Field ;
    rdfs:label "Name Field" ;
    ex:hasName "name" ;
    ex:hasType "string" ;
    ex:hasMaxLength 255 .

ex:EmailField a ex:Field ;
    rdfs:label "Email Field" ;
    ex:hasName "email" ;
    ex:hasType "string" ;
    ex:isUnique true .

ex:CreatedField a ex:Field ;
    rdfs:label "Created Timestamp" ;
    ex:hasName "created" ;
    ex:hasType "datetime" ;
    ex:isAutoGenerated true .