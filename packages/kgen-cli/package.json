{
  "name": "@kgen/cli",
  "version": "0.1.0",
  "description": "KGEN Command Line Interface for knowledge graph compilation",
  "type": "module",
  "main": "./src/cli.js",
  "bin": {
    "kgen": "./bin/kgen.js"
  },
  "exports": {
    ".": "./src/cli.js",
    "./commands": "./src/commands/index.js",
    "./utils": "./src/utils/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .js,.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist",
    "dev": "node --watch bin/kgen.js"
  },
  "dependencies": {
    "@kgen/core": "workspace:*",
    "citty": "^0.1.5",
    "consola": "^3.2.3",
    "defu": "^6.1.3",
    "c12": "^1.5.1",
    "pathe": "^1.1.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "src/",
    "bin/",
    "dist/",
    "README.md"
  ],
  "keywords": [
    "cli",
    "knowledge-graph",
    "kgen",
    "rdf",
    "code-generation"
  ],
  "license": "MIT"
}