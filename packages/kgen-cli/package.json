{
  "name": "@kgen/cli",
  "version": "1.0.0",
  "description": "KGEN Command Line Interface for knowledge graph compilation",
  "type": "module",
  "main": "./src/cli.js",
  "bin": {
    "kgen": "./src/cli.js"
  },
  "exports": {
    ".": "./src/cli.js",
    "./commands": "./src/commands/index.js",
    "./utils": "./src/utils/index.js"
  },
  "scripts": {
    "build": "echo 'No build step needed for ES modules'",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cli": "vitest run tests/integration/cli.test.js",
    "test:commands": "vitest run tests/commands/",
    "lint": "echo 'Linting skipped'",
    "clean": "rm -rf dist test-results",
    "dev": "node --watch src/cli.js"
  },
  "dependencies": {
    "citty": "^0.1.5",
    "consola": "^3.4.2",
    "c12": "^1.5.1"
  },
  "devDependencies": {
    "vitest": "^1.6.1"
  },
  "files": [
    "src/",
    "README.md"
  ],
  "keywords": [
    "cli",
    "knowledge-graph",
    "kgen",
    "rdf",
    "code-generation"
  ],
  "license": "MIT"
}