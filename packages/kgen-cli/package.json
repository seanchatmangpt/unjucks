{
  "name": "@kgen/cli",
  "version": "1.0.0",
  "description": "KGEN Command Line Interface for knowledge graph compilation",
  "type": "module",
  "bin": {
    "kgen": "./dist/cli-entry.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/cli-entry.mjs",
      "types": "./dist/cli-entry.d.ts"
    },
    "./cli-entry": {
      "import": "./dist/cli-entry.mjs",
      "types": "./dist/cli-entry.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cli": "vitest run tests/integration/cli.test.js",
    "test:commands": "vitest run tests/commands/",
    "lint": "echo 'Linting delegated to root'",
    "typecheck": "echo 'TypeScript removed - using unbuild for type checking and generation'",
    "clean": "rm -rf dist test-results",
    "dev": "node --watch ./dist/cli-entry.mjs"
  },
  "dependencies": {
    "@kgen/core": "workspace:*",
    "@kgen/rules": "workspace:*",
    "@kgen/templates": "workspace:*",
    "citty": "^0.1.5",
    "consola": "^3.4.2",
    "c12": "^1.5.1"
  },
  "devDependencies": {
    "vitest": "^1.6.1",
    "unbuild": "^2.0.0"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "keywords": [
    "cli",
    "knowledge-graph",
    "kgen",
    "rdf",
    "code-generation"
  ],
  "license": "MIT"
}
