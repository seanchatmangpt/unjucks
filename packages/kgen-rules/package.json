{
  "name": "@kgen/rules",
  "version": "1.0.0",
  "description": "KGEN N3.js rule packages for semantic reasoning and compliance with single validator entry point",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/rules-entry.mjs",
      "types": "./dist/rules-entry.d.ts"
    },
    "./validator": {
      "import": "./dist/engine/rules-validator.mjs",
      "types": "./dist/engine/rules-validator.d.ts"
    },
    "./api-governance": "./api-governance/index.n3",
    "./hipaa-core": "./hipaa-core/index.n3",
    "./sox-compliance": "./sox-compliance/index.n3"
  },
  "files": [
    "dist/",
    "tests/",
    "api-governance/",
    "hipaa-core/",
    "sox-compliance/",
    "README.md"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "node --test tests/**/*.test.js",
    "test:validator": "node --test tests/validator.test.js",
    "validate": "node -e \"import('./index.js').then(m => console.log('Validator loaded successfully'))\"",
    "demo": "node demo.js",
    "reasoning": "node scripts/test-reasoning.js",
    "typecheck": "echo 'TypeScript removed - using unbuild for type checking and generation'"
  },
  "keywords": [
    "kgen",
    "rules",
    "n3",
    "reasoning",
    "semantic-web",
    "compliance",
    "governance",
    "shacl",
    "validation"
  ],
  "author": "KGEN Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kgen/kgen.git",
    "directory": "packages/kgen-rules"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "n3": "^1.26.0",
    "rdf-validate-shacl": "^0.5.5",
    "@rdfjs/data-model": "^1.3.4",
    "@rdfjs/dataset": "^1.1.1"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "unbuild": "^2.0.0"
  }
}
