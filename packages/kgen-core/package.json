{
  "name": "@kgen/core",
  "version": "0.1.0",
  "description": "KGEN core engine for RDF graph processing and artifact generation",
  "type": "module",
  "main": "./src/engine.js",
  "exports": {
    ".": "./src/engine.js",
    "./rdf": "./src/rdf/index.js",
    "./rdf/canonical-processor": "./src/rdf/canonical-processor.js",
    "./rdf/graph-indexer": "./src/rdf/graph-indexer.js",
    "./rdf/enhanced-processor": "./src/rdf/enhanced-processor.js",
    "./semantic": "./src/semantic/processor.js",
    "./semantic/processor": "./src/semantic/processor.js",
    "./graph": "./src/graph/index.js",
    "./compiler": "./src/compiler/index.js",
    "./provenance": "./src/provenance/index.js",
    "./types": "./src/types/index.js",
    "./cache": "./src/cache/index.js",
    "./validation": "./src/validation/index.js",
    "./config": "./src/config/index.js",
    "./utils": "./src/utils/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .js,.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "n3": "^1.17.2",
    "rdf-ext": "^2.5.0",
    "crypto-js": "^4.2.0",
    "c12": "^1.4.2",
    "zod": "^3.22.4",
    "mkdirp": "^3.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@types/crypto-js": "^4.2.0"
  },
  "peerDependencies": {
    "@kgen/rules": "workspace:*",
    "@kgen/templates": "workspace:*"
  },
  "files": [
    "src/",
    "dist/",
    "README.md"
  ],
  "keywords": [
    "rdf",
    "knowledge-graph",
    "compiler",
    "provenance"
  ],
  "license": "MIT"
}