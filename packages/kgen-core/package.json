{
  "name": "@kgen/core",
  "version": "1.0.0",
  "description": "Production-ready RDF processing core for KGEN with content-addressed hashing",
  "type": "module",
  "main": "src/rdf/index.js",
  "exports": {
    ".": "./src/rdf/index.js",
    "./graph-processor": "./src/rdf/graph-processor.js",
    "./namespace-manager": "./src/rdf/namespace-manager.js",
    "./serializers": "./src/rdf/serializers.js",
    "./hash-calculator": "./src/rdf/hash-calculator.js",
    "./project": "./src/project/index.js",
    "./project/lockfile-generator": "./src/project/lockfile-generator.js",
    "./project/attestation-bundler": "./src/project/attestation-bundler.js",
    "./project/version-manager": "./src/project/version-manager.js",
    "./project/reproducibility-verifier": "./src/project/reproducibility-verifier.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "echo 'No build step needed for ES modules'",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "keywords": [
    "rdf",
    "semantic-web",
    "graph",
    "hashing",
    "content-addressing",
    "knowledge-graph",
    "n3",
    "turtle",
    "serialization",
    "kgen"
  ],
  "author": "KGEN Team",
  "license": "MIT",
  "dependencies": {
    "n3": "^1.17.2",
    "crypto": "^1.0.1",
    "consola": "^3.4.2",
    "uuid": "^10.0.0",
    "semver": "^7.5.4",
    "archiver": "^6.0.1",
    "node-stream-zip": "^1.15.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "eslint": "^8.56.0"
  },
  "peerDependencies": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".js"],
    "globals": {
      "transform": {}
    },
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kgen-dev/unjucks.git",
    "directory": "packages/kgen-core"
  },
  "bugs": {
    "url": "https://github.com/kgen-dev/unjucks/issues"
  },
  "homepage": "https://github.com/kgen-dev/unjucks/tree/main/packages/kgen-core"
}