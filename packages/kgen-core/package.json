{
  "name": "@kgen/core",
  "version": "0.1.0",
  "description": "KGEN core engine for RDF graph processing and artifact generation",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/core.mjs",
      "types": "./dist/core.d.ts"
    },
    "./engine": {
      "import": "./dist/engine.mjs",
      "types": "./dist/engine.d.ts"
    },
    "./rdf": {
      "import": "./dist/rdf/rdf-entry.mjs",
      "types": "./dist/rdf/rdf-entry.d.ts"
    },
    "./rdf/canonical-processor": {
      "import": "./dist/rdf/canonical-processor.mjs",
      "types": "./dist/rdf/canonical-processor.d.ts"
    },
    "./rdf/graph-indexer": {
      "import": "./dist/rdf/graph-indexer.mjs",
      "types": "./dist/rdf/graph-indexer.d.ts"
    },
    "./rdf/enhanced-processor": {
      "import": "./dist/rdf/enhanced-processor.mjs",
      "types": "./dist/rdf/enhanced-processor.d.ts"
    },
    "./semantic": {
      "import": "./dist/semantic/semantic-processor.mjs",
      "types": "./dist/semantic/semantic-processor.d.ts"
    },
    "./semantic/processor": {
      "import": "./dist/semantic/semantic-processor.mjs",
      "types": "./dist/semantic/semantic-processor.d.ts"
    },
    "./graph": {
      "import": "./dist/graph/graph-entry.mjs",
      "types": "./dist/graph/graph-entry.d.ts"
    },
    "./compiler": {
      "import": "./dist/compiler/compiler-entry.mjs",
      "types": "./dist/compiler/compiler-entry.d.ts"
    },
    "./provenance": {
      "import": "./dist/provenance/provenance-entry.mjs",
      "types": "./dist/provenance/provenance-entry.d.ts"
    },
    "./types": {
      "import": "./dist/types/types-entry.mjs",
      "types": "./dist/types/types-entry.d.ts"
    },
    "./cache": {
      "import": "./dist/cache/cache-entry.mjs",
      "types": "./dist/cache/cache-entry.d.ts"
    },
    "./validation": {
      "import": "./dist/validation/validation-entry.mjs",
      "types": "./dist/validation/validation-entry.d.ts"
    },
    "./config": {
      "import": "./dist/config/config-entry.mjs",
      "types": "./dist/config/config-entry.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/utils-entry.mjs",
      "types": "./dist/utils/utils-entry.d.ts"
    },
    "./cas": {
      "import": "./dist/cas/cas-entry.mjs",
      "types": "./dist/cas/cas-entry.d.ts"
    },
    "./cas/atomic-storage": {
      "import": "./dist/cas/atomic-storage-engine.mjs",
      "types": "./dist/cas/atomic-storage-engine.d.ts"
    },
    "./deterministic": {
      "import": "./dist/deterministic/deterministic-entry.mjs",
      "types": "./dist/deterministic/deterministic-entry.d.ts"
    },
    "./drift": {
      "import": "./dist/drift/drift-entry.mjs",
      "types": "./dist/drift/drift-entry.d.ts"
    },
    "./shacl": {
      "import": "./dist/shacl/shacl-validator.mjs",
      "types": "./dist/shacl/shacl-validator.d.ts"
    },
    "./shacl/reporter": {
      "import": "./dist/shacl/shacl-reporter.mjs",
      "types": "./dist/shacl/shacl-reporter.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:deterministic": "vitest run tests/unit/deterministic.test.js",
    "test:integration": "vitest run tests/integration/",
    "test:performance": "vitest run tests/performance/ --testTimeout=30000",
    "lint": "echo 'Linting delegated to root'",
    "typecheck": "echo 'TypeScript removed - using unbuild for type checking and generation'",
    "clean": "rm -rf dist test-results"
  },
  "dependencies": {
    "n3": "^1.17.2",
    "rdf-ext": "^2.5.0",
    "rdf-validate-shacl": "^0.5.3",
    "@tpluscode/rdf-string": "^1.3.0",
    "crypto-js": "^4.2.0",
    "c12": "^1.4.2",
    "zod": "^3.22.4",
    "mkdirp": "^3.0.1",
    "yaml": "^2.3.4",
    "sparqljs": "^3.7.3",
    "gray-matter": "^4.0.3",
    "nunjucks": "^3.2.4",
    "tar-stream": "^3.1.7",
    "pako": "^2.1.0",
    "hash-wasm": "^4.12.0",
    "multiformats": "^13.0.0",
    "consola": "^3.2.3",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "vitest": "^1.0.0",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {},
  "files": [
    "dist/",
    "README.md"
  ],
  "keywords": [
    "rdf",
    "knowledge-graph",
    "compiler",
    "provenance",
    "esm"
  ],
  "license": "MIT"
}
