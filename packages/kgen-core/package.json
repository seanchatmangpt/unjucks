{
  "name": "@kgen/core",
  "version": "0.1.0",
  "description": "KGEN core engine for RDF graph processing and artifact generation",
  "type": "module",
  "main": "./src/engine.js",
  "exports": {
    ".": "./src/engine.js",
    "./rdf": "./src/rdf/index.js",
    "./rdf/canonical-processor": "./src/rdf/canonical-processor.js",
    "./rdf/graph-indexer": "./src/rdf/graph-indexer.js",
    "./rdf/enhanced-processor": "./src/rdf/enhanced-processor.js",
    "./semantic": "./src/semantic/processor.js",
    "./semantic/processor": "./src/semantic/processor.js",
    "./graph": "./src/graph/index.js",
    "./compiler": "./src/compiler/index.js",
    "./provenance": "./src/provenance/index.js",
    "./types": "./src/types/index.js",
    "./cache": "./src/cache/index.js",
    "./validation": "./src/validation/index.js",
    "./config": "./src/config/index.js",
    "./utils": "./src/utils/index.js",
    "./cas": "./src/cas/index.js",
    "./cas/atomic-storage": "./src/cas/atomic-storage-engine.js",
    "./deterministic": "./src/deterministic/index.js",
    "./drift": "./src/drift/index.js",
    "./shacl": "./src/shacl/validator.js",
    "./shacl/reporter": "./src/shacl/reporter.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:deterministic": "vitest run tests/unit/deterministic.test.js",
    "test:integration": "vitest run tests/integration/",
    "test:performance": "vitest run tests/performance/ --testTimeout=30000",
    "lint": "eslint src/ --ext .js,.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "clean": "rm -rf dist test-results"
  },
  "dependencies": {
    "n3": "^1.17.2",
    "rdf-ext": "^2.5.0",
    "rdf-validate-shacl": "^0.5.3",
    "@tpluscode/rdf-string": "^1.3.0",
    "crypto-js": "^4.2.0",
    "c12": "^1.4.2",
    "zod": "^3.22.4",
    "mkdirp": "^3.0.1",
    "yaml": "^2.3.4",
    "sparqljs": "^3.7.3",
    "gray-matter": "^4.0.3",
    "nunjucks": "^3.2.4",
    "hash-wasm": "^4.12.0",
    "multiformats": "^13.0.0",
    "consola": "^3.2.3",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@types/crypto-js": "^4.2.0"
  },
  "peerDependencies": {
  },
  "files": [
    "src/",
    "dist/",
    "README.md"
  ],
  "keywords": [
    "rdf",
    "knowledge-graph",
    "compiler",
    "provenance"
  ],
  "license": "MIT"
}