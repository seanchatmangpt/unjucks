@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix ex: <http://example.org/> .

ex:Person1 a foaf:Person ;
    foaf:name "John Doe" ;
    foaf:age 30 ;
    foaf:email "john@example.org" ;
    foaf:knows ex:Person2 .

ex:Person2 a foaf:Person ;
    foaf:name "Jane Smith" ;
    foaf:age 28 ;
    foaf:email "jane@example.org" ;
    foaf:knows ex:Person1 .

ex:Organization1 a foaf:Organization ;
    foaf:name "Example Corp" ;
    dc:description "A sample organization for testing" ;
    foaf:member ex:Person1, ex:Person2 .

ex:Project1 a ex:Project ;
    dc:title "RDF Processing Project" ;
    dc:description "A project for testing RDF processing capabilities" ;
    ex:hasContributor ex:Person1 ;
    ex:hasContributor ex:Person2 ;
    ex:startDate "2024-01-01"^^<http://www.w3.org/2001/XMLSchema#date> .