  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanchatmangpt/unjucks.git",
    "directory": "packages"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
