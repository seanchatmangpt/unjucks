{
  "app": {
    "name": "unjucks-enterprise",
    "version": "1.0.0",
    "description": "Unjucks Enterprise Application",
    "environment": "staging",
    "debug": false,
    "timezone": "UTC",
    "locale": "en-US"
  },
  "server": {
    "port": 3000,
    "host": "0.0.0.0",
    "timeout": 30000,
    "keepAliveTimeout": 65000,
    "maxConnections": 5000,
    "gracefulShutdown": {
      "enabled": true,
      "timeout": 30000
    }
  },
  "security": {
    "cors": {
      "origin": [
        "https://staging.unjucks.dev",
        "http://localhost:3000"
      ],
      "credentials": true,
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedHeaders": ["Content-Type", "Authorization", "X-Requested-With"],
      "maxAge": 86400
    },
    "rateLimit": {
      "enabled": true,
      "windowMs": 1800000,
      "maxRequests": 2000,
      "skipSuccessfulRequests": false,
      "skipFailedRequests": false,
      "standardHeaders": true,
      "legacyHeaders": false
    },
    "headers": {
      "hsts": true,
      "noSniff": true,
      "xframe": true,
      "xss": true,
      "referrerPolicy": "no-referrer",
      "contentSecurityPolicy": true
    },
    "jwt": {
      "algorithm": "HS256",
      "expiresIn": "12h"
    },
    "encryption": {
      "algorithm": "aes-256-gcm",
      "keyLength": 32
    }
  },
  "database": {
    "type": "postgresql",
    "ssl": true,
    "pool": {
      "min": 3,
      "max": 20,
      "acquireTimeoutMillis": 60000,
      "idleTimeoutMillis": 30000,
      "createTimeoutMillis": 30000
    },
    "retries": {
      "max": 3,
      "backoffBase": 300,
      "backoffExponent": 2
    }
  },
  "cache": {
    "type": "redis",
    "ttl": 3600,
    "maxSize": 50000,
    "redis": {
      "host": "localhost",
      "port": 6379,
      "db": 1,
      "keyPrefix": "unjucks:staging:",
      "retryDelayOnFailover": 100
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "enableAudit": true,
    "enablePerformance": true,
    "enableSensitiveData": false,
    "retention": {
      "days": 30,
      "maxSize": "500MB",
      "maxFiles": 15
    },
    "transports": ["console", "file"]
  },
  "monitoring": {
    "enabled": true,
    "metrics": {
      "enabled": true,
      "endpoint": "/metrics",
      "interval": 15000,
      "prometheus": true,
      "customMetrics": []
    },
    "health": {
      "enabled": true,
      "endpoint": "/health",
      "detailed": true,
      "checks": ["database", "cache", "memory"]
    },
    "tracing": {
      "enabled": true,
      "service": "unjucks-staging",
      "sampleRate": 0.5
    }
  },
  "features": {
    "enabled": true,
    "provider": "local",
    "refreshInterval": 30000,
    "flags": {
      "config.hot-reload": false,
      "config.validation": true,
      "config.secrets": true,
      "logging.structured": true,
      "monitoring.detailed": true,
      "security.enhanced": true,
      "cache.distributed": true,
      "unjucks.templates.cache": true,
      "unjucks.generators.parallel": true,
      "unjucks.validation.strict": true,
      "unjucks.watch.enabled": false,
      "unjucks.rdf.support": true
    },
    "rollouts": {
      "unjucks.generators.parallel": {
        "enabled": true,
        "percentage": 50,
        "userGroups": ["beta-testers", "performance-testers"],
        "conditions": [
          {
            "attribute": "environment",
            "operator": "eq",
            "value": "staging"
          }
        ]
      },
      "security.enhanced": {
        "enabled": true,
        "percentage": 100,
        "userGroups": [],
        "conditions": []
      }
    }
  },
  "secrets": {
    "provider": "vault",
    "vault": {
      "endpoint": "https://vault.staging.unjucks.dev:8200",
      "mount": "secret",
      "version": "v2",
      "timeout": 5000
    },
    "caching": {
      "enabled": true,
      "ttl": 900
    }
  },
  "hotReload": {
    "enabled": false,
    "watchPaths": ["./config"],
    "excludePaths": ["./config/secrets", "./config/runtime.json"],
    "debounceMs": 1000,
    "restartOnChange": ["server", "database", "security"],
    "safeReload": ["logging", "monitoring", "features"]
  },
  "unjucks": {
    "templatesDir": "./templates",
    "outputDir": "./output",
    "defaultEngine": "nunjucks",
    "engines": {
      "nunjucks": {
        "autoescape": true,
        "throwOnUndefined": true,
        "trimBlocks": true,
        "lstripBlocks": true
      },
      "ejs": {
        "delimiter": "%",
        "openDelimiter": "<",
        "closeDelimiter": ">",
        "cache": true
      }
    },
    "generators": {
      "parallel": true,
      "maxConcurrency": 6,
      "timeout": 45000
    },
    "validation": {
      "enabled": true,
      "strict": true,
      "schema": "./config/schemas/template-schema.json"
    },
    "watch": {
      "enabled": false,
      "patterns": ["**/*.njk", "**/*.ejs", "**/*.yml"],
      "ignored": ["node_modules/**", ".git/**"]
    },
    "rdf": {
      "enabled": true,
      "formats": ["turtle", "rdf-xml", "json-ld"],
      "namespaces": {
        "foaf": "http://xmlns.com/foaf/0.1/",
        "dc": "http://purl.org/dc/elements/1.1/",
        "unjucks": "https://unjucks.dev/ns/"
      }
    }
  }
}