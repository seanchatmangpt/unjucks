{
  "documents": [
    {
      "template": "compliance-report",
      "type": "latex",
      "data": "./examples/document-data/compliance-audit.json",
      "category": "compliance",
      "compliance": true,
      "complianceFramework": "enterprise",
      "standards": ["ISO-27001", "SOX", "GDPR"]
    },
    {
      "template": "technical-specification", 
      "type": "latex",
      "data": "./examples/document-data/technical-spec.json",
      "category": "technical",
      "output": "api-specification.tex"
    },
    {
      "template": "data-matrix",
      "type": "excel", 
      "data": "./examples/document-data/compliance-audit.json",
      "category": "compliance",
      "compliance": true,
      "output": "compliance-matrix.xlsx"
    }
  ],
  "globalSettings": {
    "templatesDir": "_templates/documents",
    "outputDir": "./generated/documents",
    "enableAttestations": true,
    "compilePdf": true,
    "validateDocuments": true
  }
}